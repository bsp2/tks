
geo 640 480
;   0    1    2    3    4    5    6    7
pal #edb #8ca #9ba #7cb #fff #9dc #555 #333

; clear screen
f 0

; define path
p mypath
M -100 -100
r 200 200
z


M 320 240

i 1
k fill

q rotate(0 -20 0) translate(120 0 0) scale(0.01) rotate(-65 0 0) translate(0 0 1.3) scale(40 60 1) scale(1.5 1.5 1)
d3 mypath

q rotate(0 20 0) translate(-120 0 0) scale(0.01) rotate(-65 0 0) translate(0 0 1.3) scale(40 60 1)  scale(1.5 1.5 1)
d3 mypath


; draw stroked path
i 6
h m
k 4

q rotate(0 -20 0) translate(120 0 0) scale(0.01) rotate(-65 0 0) translate(0 0 1.3) scale(40 60 1)  scale(1.5 1.5 1)
d3 mypath

q rotate(0 20 0) translate(-120 0 0) scale(0.01) rotate(-65 0 0) translate(0 0 1.3) scale(40 60 1)  scale(1.5 1.5 1)
d3 mypath



; draw filled path
i 2
k fill
q rotate(-65 0 0) translate(0 -25 0) scale(0.004 0.01 0.01) rotate(0 0 0) translate(0 0 1.3) scale(45 40 1) scale(1.5 1.5 1)
d3 mypath

i 7
h m
k 4
q rotate(-65 0 0) translate(0 -25 0) scale(0.004 0.01 0.01) rotate(0 0 0) translate(0 0 1.3) scale(45 40 1) scale(1.5 1.5 1)
d3 mypath
