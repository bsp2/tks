
geo 640 480
;   0    1    2    3    4    5    6    7
pal #eee #567 #768 #000 #fff #9cd #679 #498

; clear screen
f 0

; define paths
p myrect
M -60 -60
r 120 120
z

p myellipse
M -36 -36
j 96
e 72 72
z

; draw filled rect
n rotate(45) translate(320 240)
i 4
k fill
d2 myrect

; enable mask and draw filled ellipse
n rotate(45) translate(320 270)
a 4
i 5
d2 myellipse

; enable mask and draw filled ellipse
a 5
i 6
n rotate(45) translate(300 260)
d2 myellipse

; enable mask and draw filled ellipse
a 6
i 1
n rotate(45) translate(300 230)
d2 myellipse

; disable mask and draw stroked rect
n rotate(45) translate(320 240)
a off
i 3
h miter
k 2
d2 myrect
