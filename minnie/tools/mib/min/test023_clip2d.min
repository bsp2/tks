
geo 640 480
;   0    1    2    3    4    5    6    7
pal #eee #6db #ffe #efe #dde #9dc #779 #498

; clear screen
f 0

p clippath
M 150 150
r 200 200
z

; define closed concave path
pt mypath
M 100  70  ; 0
L 160 200  ; 1
L 400  90  ; 2
L 250 400  ; 3
L 220 240  ; 4
L  60 320  ; 5
z

; draw clip path (debug)
i 2
k fill
d clippath

; draw unclipped filled path (debug)
i 3
k fill
d mypath

; draw unclipped stroked path (debug)
i 4
h miter
k 3
d mypath

; set clip path
w2p clippath

; draw filled path
i 1
k fill
d mypath

; draw stroked path
i 6
h miter
k 3
d mypath
