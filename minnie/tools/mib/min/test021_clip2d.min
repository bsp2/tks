
geo 640 480

;   0    1    2    3    4    5    6    7
pal #eee #6db #ffe #efe #dde #9dc #779 #498

; clear screen
f 0

; define path
p clippath
M 200 200
r 150 150
z

; define path
p mypath
M 245 165
r 150 150
z

; draw clip path (debug)
i 2
k fill
d clippath

; draw unclipped filled path
i 3
k fill
d mypath

; draw unclipped stroked path
i 4
h m
k 6
d mypath


; set clip path
w2 clippath

; draw filled path
i 1
k fill
d mypath

; draw stroked path (post-extrude clipping)
i 6
h m
k 6
d mypath
