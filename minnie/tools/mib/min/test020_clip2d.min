
geo 640 480
pal #eee #6db #ffe #efe #dde #9dc #779 #498

; clear screen
f 0

; define path
p clippath
M 275 275
rc 75 75
;j 16
;e 75 75

;M 200 200
;L 400 300
;L 400 400
;L 200 220

z

; define path
p mypath
M 245 165
r 150 150
z

; draw clip path (debug)
i 2
k fill
d clippath

; draw unclipped filled path
i 3
k fill
d mypath

; draw unclipped stroked path
i 4
h m
k 6
d mypath

; set clip path
w2p clippath

; draw filled path
i 1
k fill
;d mypath

; draw stroked path (pre-extrude clipping)
i 6
h m
k 6
d mypath
