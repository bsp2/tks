
geo 640 480
pal #eee #6db #547 #7cb #fff #9dc #779 #498

; clear screen
f 0

; define closed concave path
pt mypath
M 100 140  ; 0
L 160 200  ; 1
L 500  70  ; 2
L 250 300  ; 3
L 220 240  ; 4
L 120 380  ; 5
z

M 320 240
;M 120 40

q translate(-165 -155 0) scale(0.005 0.005 1) rotate(-75 0 0) scale(125 80 0.32) translate(0 0 0.41)
;rotate(0 0 22)
;q translate(0 0 1)
w3fb


; draw filled path
i 1
k fill
d3 mypath

; draw stroked path
i 6
h miter
k 4
d3 mypath
