
geo 640 480
pal #eee #6db #ffe #efe #dde #9dc #779 #498

; clear screen
f 0

p clippath
M 200 200
r 150 150
z

p clippath_line
M 200 200
r 149 149
z

; define path
p mypath
M 245 165
r 150 150
z

; draw unclipped filled path
i 3
k fill
d mypath

; draw unclipped stroked path
i 4
h m
k 6
d mypath

; draw clip path (debug)
i 2
k fill
d clippath

; set clip path
w2 clippath

; draw filled path
i 1
k fill
d mypath

; set clip path
;  (todo) line cliprect currently needs to be reduced by 1 pixel on both axis
w2 clippath_line

; draw line path
i 6
h m
k line
d mypath
