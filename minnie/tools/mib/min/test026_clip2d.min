
geo 640 480
pal #eee #6db #ffe #efe #dde #9dc #779 #498

; clear screen
f 0

; define path
p clippath
M 275 255
;rc 75 75
j 16
e 75 75

;M 200 200
;L 400 300
;L 400 400
;L 200 220

z

; define closed concave path
pt mypath
M -120 -40  ; 0
L  80  -30  ; 1
L  30   60  ; 2
L   0    0  ; 3
L -70  140  ; 4
z

; draw clip path (debug)
i 2
k fill
d clippath

n rotate(30)
M 320 240

; draw unclipped filled path
i 3
k fill
d mypath

; draw unclipped stroked path
i 4
h m
;k line
k 6
d mypath

; set clip path
w2 clippath

; draw filled path
i 1
k fill
d mypath

; draw stroked path (pre-extrude clipping)
i 6
h m
;k line
k 6
d mypath
