
geo 640 480
pal #eee #6db #547 #7cb #fff #9dc #779 #498

; clear screen
f 0

gxf -150 -160 2 2

; define closed concave path
pt mypath
M 200 200  ; 0
L 400 210  ; 1
L 350 300  ; 2
L 320 240  ; 3
L 250 380  ; 4
z

; draw filled path
i 1
k fill
d mypath

; draw stroked path
i 6
h miter butt
;k line
k 2
d mypath
