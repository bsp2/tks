
geo_scale 1
geo 800 600
pal #eee #6db #547 #7cb #fff #9dc #779 #498

; clear screen
f 0

gxf 0 0 1.7 1.7

; define path
p mypath
M -200 -10
r 400 20
z

; draw stroked paths
i 6
h m
k 6
n scale(0.4 0.4) rotate(45) translate(400 300)
d2 mypath

n scale(0.6 0.6) rotate(22.5) translate(400 300)
d2 mypath

n scale(0.75 0.75) rotate(-22.5) translate(400 300)
d2 mypath

n scale(1.0 1.0) rotate(-45.0) translate(400 300)
d2 mypath


; draw filled path
i 1
k fill

n scale(0.4 0.4) rotate(45) translate(400 300)
d2 mypath

n scale(0.6 0.6) rotate(22.5) translate(400 300)
d2 mypath

n scale(0.75 0.75) rotate(-22.5) translate(400 300)
d2 mypath

n scale(1.0 1.0) rotate(-45.0) translate(400 300)
d2 mypath
