
; tests path reversal (CCW=>CW)

; geo_scale / geo_div scales framebuffers, blits, and paths
geo_div 1

; seg_scale scales number of path subdivision segments (splines, ellipses, round line joints)
;seg_scale 0.125

geo 640 480
pal #eee #6db #547 #7cb #fff #9dc #779 #498

; clear screen
f 0

; set path transform (pre-process)
gxf 0 0 0.7 0.7 0 0

; 10 vertices, CCW winding (=> reverse)
pt star
svg M 0 100 L 35.2671 48.541 L 95.1057 30.9017 L 57.0634 -18.541 L 58.7785 -80.9017 L -5.24537e-06 -60 L -58.7785 -80.9017 L -57.0634 -18.541 L -95.1057 30.9017 L -35.2671 48.541 z
;svg M 0 100 L 52.9007 72.8115 L 95.1057 30.9017 L 85.5951 -27.8115 L 58.7785 -80.9017 L -7.86805e-06 -90 L -58.7785 -80.9017 L -85.5951 -27.8115 L -95.1057 30.9017 L -52.9006 72.8116 z
;svg M 0 100 L 41.145 56.6312 L 95.1057 30.9017 L 66.574 -21.6312 L 58.7785 -80.9017 L -6.11959e-06 -70 L -58.7785 -80.9017 L -66.574 -21.6312 L -95.1057 30.9017 L -41.1449 56.6312 z
z

gxf 0 0 1 1 0 0

; draw stroked
i 6
k 5
seg 8
h miter

M 320 240
d star

; draw filled
i 3
k fill
seg 8
h b

M 320 240
d star
