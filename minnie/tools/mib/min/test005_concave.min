
geo 640 480
pal #eee #6db #547 #7cb #fff #9dc #779 #498

; clear screen
f 0

; define closed concave path
pt mypath
M 100 140  ; 0
L 160 200  ; 1
L 500  70  ; 2
L 250 300  ; 3
L 220 240  ; 4
L 120 380  ; 5
z

; draw filled path
i 1
k fill
d mypath

; draw stroked path
i 6
; k line
k 3.0
h miter
ml 10
d mypath
