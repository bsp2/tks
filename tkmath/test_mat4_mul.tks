#define NUM_ITER 1000000

use tkmath;

Matrix4f m1;
Matrix4f m2;
Matrix4f r;
int t;

t = milliSeconds();
loop(NUM_ITER)
{
   m1.mul(m2) => r;
}
t = milliSeconds() - t;

trace NUM_ITER+" iterations in "+t+" milliseconds => "+int((1000.0/t)*NUM_ITER)+" mat4 muls per second";


Vector4f v1;
Vector4f v2;
Vector4f rv;
t = milliSeconds();
loop(NUM_ITER)
{
   v1.add(v2) => rv;
}
t = milliSeconds() - t;

trace NUM_ITER+" iterations in "+t+" milliseconds => "+int((1000.0/t)*NUM_ITER)+" vec4 adds per second";

