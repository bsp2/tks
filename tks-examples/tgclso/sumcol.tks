/*
 *  http://www.bagley.org/~doug/shootout/bench/sumcol/
 *
 * 0.223s (amd athlon 2500+)
 */
int i=0;

int t=milliSeconds();
loop(100)
{
    String s; s.loadLocal("sumcol.txt", true);
    int j;
    s.split('\n'); String k; foreach k in s i+=int(k);
    s.freeStack();
}
t=milliSeconds()-t;
trace "t="+t;
trace "i="+i;
