

function Tak(local float x, local float y, local float z) {
    if (y >= x) return z;
    else return Tak(Tak(x-1.0,y,z), Tak(y-1.0,z,x), Tak(z-1.0,x,y));
}

function main() {
    int n=0;
    if(!Arguments.numElements)
	n=10;
    else
	n=Arguments[0];
    print "n="+n;
    int t=milliSeconds();
    print Tak(n*3.0, n*2.0, n*1.0);
    t=milliSeconds()-t;
}


