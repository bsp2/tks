
// ---- these two function basically do the same thing:
//      typecast an integer to a String and return a volatile (i.e. deletable) object.
//

function ftest2(int k) {
    String s=k;
    return String(s); // create a copy of "s"
}

function ftest(int k) {
    local String s=k;
    return deref s;
}

print ftest(2);
print ftest2(3);
