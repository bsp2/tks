
int i;

compile {
   i = 0;
   loop(1000)
   {
      if(++i > 100)
         break;
   }
}
   
trace "loop: i="+i;


compile {
   for(i=0; i<1000; i++)
   {
      if(++i > 100)
         break;
   }
}

trace "for: i="+i;


compile {
   i = 0;
   do
   {
      if(++i > 100)
         break;
   } while(i < 1000);
}

trace "do..while: i="+i;


compile {
   i = 0;
   while(i < 1000)
   {
      if(++i > 100)
         break;
   }
}

trace "while: i="+i;
