float f;

f=3.40282347e+38; // maximum 32bit float value
print f;
f=-3.40282347e+38;
print f;

f=1.17549435e-38;  // minimum 32bit float value
print f;

int i;

i=2147483647;      // maximum 32bit integer value
print i;

i=(-2147483647-1); // minimum 32bit integer value
print i;

Long l="9223372036854775807";
print l;
print -l;

Integer i32=42;
print -i32;


