
class MyClass {
   String s = "hello, world.";

   MyClass() {
      trace "MyClass()";
   }

   test() {
      trace "MyClass::test: s="+s;
   }
}

function Test() {
   MyClass *c, *d, *e<=new MyClass, *f;

   trace "c="+#(c);
   trace "d="+#(d);
   trace "e="+#(e);
   trace "f="+#(f);
}

Test();


