module Main;

function main() {
    int argc=Arguments.numElements;
    trace "[...] got "+argc+" args";
    for(int i=0; i<argc; i++)
	trace "[...] arg["+i+"]=\""+Arguments[i]+"\"";

    String arg; i=0;
    foreach arg in Arguments
        trace "[...] foreach arg["+i+++"]=\""+arg+"\"";

    trace "TKS_PLUGIN_DIR="+getenv("TKS_PLUGIN_PATH");
    trace "TKS_MODULE_PATH="+getenv("TKS_MODULE_PATH");
    trace "USER="+getenv("USER");
}
