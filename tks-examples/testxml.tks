String s;

if(!s.loadLocal("input.xml", 1))
   die "cannot load input file.";

TreeNode troot<=s.parseXML();
if(troot)
{
    trace "tree has "+troot.numNodes+" nodes.";
    HashTable ht;
    troot.writeToHashTable(ht);
    String key; foreach key in ht {
       print "ht[\""+key+"\"]="+ht[key];
    }
}
else
   die "parseXML failed.";



