HashTable values<=#["i"=42, "f"=3.14, "s"="hello, world."];

function vreturn(String _arg) {
    if(values.exists(_arg))
	return values[_arg];
    else
	return "vreturn(): no such element \""+_arg+"\".";
}

print vreturn("i");
print vreturn("f");
print vreturn("s");
print vreturn("xyz");
