Script sc;

trace "xxx load \"tempscript_class2.tks\"";

String s_script;
s_script.loadLocal("tempscript_class2.tks", true);

int iter = 1;
loop(1000)
{
   trace "---------------- iter="+iter+" --------------------";
   if(sc.load(s_script))
   {
      trace "loaded.";
      sc.eval();
      trace "unload BEGIN";
      sc.unload();
      trace "unload END";
   }
   else
      trace "sc.load() failed";
   iter++;
}
