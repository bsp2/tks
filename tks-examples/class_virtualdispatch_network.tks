
module Main;

class FloatingLayer {
   public method hide() {
      trace "FloatingLayer::hide";
   }
   x() { hide(); }
}

class TableCellEditor : FloatingLayer {
   public virtual hide() {
      trace "TableCellEditor::hide";
   }
   y() { hide(); }
}

class FloatParamEditor : FloatingLayer {
   public virtual hide() {
      trace "FloatParamEditor::hide";
   }
   z() { hide(); }
}

class TableCellEditorFloatParam : TableCellEditor, FloatParamEditor {

   w() { hide(); }
}

trace "-----------------------------------";
trace "class = FloatingLayer";
FloatingLayer fl;
fl.hide();
fl.x();
