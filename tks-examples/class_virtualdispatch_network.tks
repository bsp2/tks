fl <= tce;
fl.hide();
fl.x();
trace "\n";


trace "-----------------------------------";
trace "class = FloatParamEditor";
FloatParamEditor fpe;
fpe.hide();
fpe.x();
fpe.z();
trace "  as FloatingLayer:";
fl <= fpe;
fl.hide();
fl.x();
trace "\n";

trace "-----------------------------------";
trace "class = TableCellEditorFloatParam";
TableCellEditorFloatParam tcefp;
tcefp.hide();
tcefp.x();
tcefp.y();
tcefp.z();
tcefp.w();
trace "  as FloatingLayer:";
fl <= tcefp;
fl.hide();
fl.x();
trace "  as FloatParamEditor:";
fpe <= tcefp;
fpe.hide();
fpe.x();
trace "  as TableCellEditor:";
tce <= tcefp;
tce.hide();
tce.x();
trace "\n";


