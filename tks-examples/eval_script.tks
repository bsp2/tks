// (note) short-hand version of "tempscript_printhook.tks" example

// % tks eval_script.tks
// hello, world.
//   output: hello, world.

String s_script <= 
   "trace  \"hello, world.\\n\";\n"
   // "trace un.declared;\n"
   // "& tracex \"xxhello, world.\\n\";\n"  // raise parse error
   // "tracex \"xxhello, world.\\n\";\n"    // raise resolve error
   ;

String *line;
foreach line in TKS.evalScript(s_script).splitChar('\n')
  trace "  output: "+line;
