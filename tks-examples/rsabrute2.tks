// RealCalcLoop
compile {
    int itime=milliSeconds();
    int Res=0;
    int x,y,z;
    x=1;
    loop(100)
	{
	    y=1;
	    loop(1000)
		{
		    z=1;
		    loop(1000)
			Res+=((x+y+z++) % 4);
		    y++;
		}
	    x++;
	}
}
itime=milliSeconds()-itime;
trace "ticks="+itime;


