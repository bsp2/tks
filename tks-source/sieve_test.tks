module Main;

int flags[8191]; flags.numElements=8191;

compile {
   int count;
   local int size= 8190;
   local int i, prime, k, iter;
   trace "Eratosthenes Sieve prime number calculation";
    
    trace "10 iterations<br>";
//      int starttime=SDL.ticks;
    for (iter = 1; iter <= 100000; iter++)
	{
	    count = 0;
	    for (i = 0; i <= size; i++) flags[i] = true;
	    for (i = 0; i <= size; i++)
		{
		    if (flags[i])
			{
			    prime = i + i + 3;
			    k = i + prime;
			    while (k <= size)
				{
				    flags[k] = false;
				    k += prime;
				}
			    count ++;
			}
		}
	}
//      int elapsedtime = SDL.ticks- starttime;
    trace count + " primes";
//      trace "elapsed time = " + elapsedtime;
}

