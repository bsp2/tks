
module MTestFloatCompare;

class TestFloatCompare : TestCase {

   testCompare() {
      float f = 3.1415;
      assertqFloat(3.1415, f);
   }

   testNot() {
      float f = 0;
      assertq(true, !f);
   }

   testCompare0() {
      float f = 1;
      assertq(true, 0 != f);
      assertq(true, f != 0);
      assertq(true, f);
   }

   testCompareObject() {
      Double d = Double.Newf(2)/3;
      float f = 2.0/3;
      assertq(true, f == float(d));
   }
}
