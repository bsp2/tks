
module MTestFloatArray;


class TestFloatArray : TestCase {

   testAutoResize() {

      FloatArray fa;
      fa.free();
      assertInt(0, fa.numElements, "fa is empty");

      int i=0, j=0;
      loop(1000)
      {
         fa[i++] = j;
         j += 10;
      }

      assertInt(1000, fa.numElements, "fa has 1000 elements");
   }

}
