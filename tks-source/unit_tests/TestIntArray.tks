
module MTestIntArray;


class TestIntArray : TestCase {

   testAutoResize() {

      IntArray ia;
      ia.free();
      assertInt(0, ia.numElements, "ia is empty");

      int i=0, j=0;
      loop(1000)
      {
         ia[i++] = j;
         j += 10;
      }

      assertInt(1000, ia.numElements, "ia has 1000 elements");
   }

}
