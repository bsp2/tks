      _inHz.emitInt(this, _out, _sVar);
      _out.append("if("+_sVar+" > 0)\n");
      _out.append("{\n");
      // (todo) rcp (avoid div)
      _out.append("   "+_sVar+" = "+int(sample_rate)+" / "+_sVar+";\n");
