         emitSlewInt(_out, _sDstVar,
                     sVarA,
                     sVarB,
                     sSclUp,
                     sSclDown
                     );
      }
      else
      {
         // neither 'a' or 'b' are connected => previous input is 'b', 'a' is last seen output
         emitSlewInt(_out, _sDstVar,
