class CyModuleReadBus : CyModule {
   // 'rbl', 'rbr'
   boolean b_right;

   // <method_init.png>
   public virtual init() {
      CyModule::init();

      addInput("nri", 0.0, 1,     32  );
      addInput("nrf", 0.0, 0.1,   0.32);
   }

