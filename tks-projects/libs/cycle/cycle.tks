      }
      else
      {
         fVSync = -1.0f;
         sVarVSync <= _script.allocTmpVarInt(_out, "vsync");
         inVSync.emitInt(_script, _out, sVarVSync);
         if(b_export_c)
            sOut.append(sTmp+" = mulsw("+sTmp+", "+sVarVSync+") >> "+FX_SHR+";\n");
