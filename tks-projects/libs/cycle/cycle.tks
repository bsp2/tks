         float_xfade_buf.allocAndFill(MAX_XFADE_FRAMES * MAX_OVERSAMPLE_FACTOR, 0);
      }

      oversample_factor = 1.0;
      oversample_quality = 4;

      dither_mode = 0;

      num_audio_lanes = 0;
   }

