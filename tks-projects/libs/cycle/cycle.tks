      if(bIn)
         sIndex.append("Dmulsw_shr("+_sDstVar+", "+curveSzMask+")");  // 0..1 => 0..size-1
      else
         sIndex.append("("+_sDstVar+sIndexShift+")"); // 0..1 => 0..size

      // trace "xxx inClamp.def_value="+inClamp.getConstVal();
      if(inLin.getConstVal() >= 0.5)
      {
