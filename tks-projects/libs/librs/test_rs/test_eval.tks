int tStart = milliSeconds();
int numIter = 100000;
int iter = 0;
loop(numIter)
{
   TKS.evalScript("int i = "+(++iter)+";");
}
int tDelta = milliSeconds() - tStart;
float cps = (1000.0f * numIter) / tDelta;
trace "test_eval: [...] "+iter+" iterations in "+tDelta+" ms => "+cps+" iterations / second";
