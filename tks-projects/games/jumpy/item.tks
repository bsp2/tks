module MItem;

namespace jumpy;

class Item : BoxEntity {

   public method initItem() {
      initEntity();
      reset_b_active  = true;
      reset_b_visible = true;
      b_collectable   = true;
   }

   public virtual resetEntity() {
      BoxEntity::resetEntity();

      b_collectable = true;
   }

}
