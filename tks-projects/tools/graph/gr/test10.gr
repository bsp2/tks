
style def {
  bgcolor      = #dee
  fgcolor      = #111
  shape        = round
  border_width = 1
  border_color = #999
  ipad         = "1;4;1;4"
}

graph {
  grid  = 64,8
  ipad  = 2
  slant = 1

  a->b->c
  d->c  // swap dir

  e->b  // swap dir
  f->c  // swap dir
  g->f  // swap dir

  e "e"
}
