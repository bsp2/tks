module MGUniformSampler2D;

use namespace ui;
use namespace gledit;


// <class.png>
class GUniformSampler2D : GUniformSampler {


   // <method_new.png>
   static New() : GUniformSampler2D {
      local GUniformSampler2D r;
      r.init();
      return deref r;
   }

   // <method.png>
   public virtual getUniformSamplerType() : int {
      return TYPE_SAMPLER2D;
   }

}
