  <!-- Knobs 9..16 -->
  <cc ctl=2  id=knob_9    caption="Knob 9"    class=value/>
  <cc ctl=3  id=knob_10   caption="Knob 10"   class=value/>
  <cc ctl=4  id=knob_11   caption="Knob 11"   class=value/>
  <cc ctl=5  id=knob_12   caption="Knob 12"   class=value/>
  <cc ctl=6  id=knob_13   caption="Knob 13"   class=value/>
  <cc ctl=7  id=knob_14   caption="Knob 14"   class=value/>
  <cc ctl=8  id=knob_15   caption="Knob 15"   class=value/>
  <cc ctl=9  id=knob_16   caption="Knob 16"   class=value/>

  <!-- Pads (C-4..F-5 => 1..12) -->
  <note nr=48 id=pad1     caption="Pad 1"     class=button/>  <!-- C-4 -->
  <note nr=50 id=pad1     caption="Pad 2"     class=button/>  <!-- D-4 -->
  <note nr=52 id=pad1     caption="Pad 3"     class=button/>  <!-- E-4 -->
  <note nr=53 id=pad1     caption="Pad 4"     class=button/>  <!-- F-4 -->
  <note nr=55 id=pad1     caption="Pad 5"     class=button/>  <!-- G-4 -->
  <note nr=57 id=pad1     caption="Pad 6"     class=button/>  <!-- A-4 -->
  <note nr=58 id=pad1     caption="Pad 7"     class=button/>  <!-- A#4 -->
  <note nr=59 id=pad1     caption="Pad 8"     class=button/>  <!-- B-4 -->
  <note nr=60 id=pad1     caption="Pad 9"     class=button/>  <!-- C-5 -->
  <note nr=62 id=pad1     caption="Pad 10"    class=button/>  <!-- D-5 -->
  <note nr=64 id=pad1     caption="Pad 11"    class=button/>  <!-- E-5 -->
  <note nr=65 id=pad1     caption="Pad 12"    class=button/>  <!-- F-5 -->

</MIDISynthProfile>
