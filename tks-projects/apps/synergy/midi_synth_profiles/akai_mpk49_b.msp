<!--
    Akai MPK49 (control bank "B", channel #2)

    Changed: 21Jan2016, 19Mar2016, 10Apr2016, 28Oct2017, 10May2020
  -->

<MIDISynthProfile
   id="mpk49_b"
   caption="Akai MPK 49 CtlBank B"
   inherit=1
   >

  <!-- Switches 9..16 -->
  <cc ctl=21 id=switch_9    caption="Switch 9"   class=button repeat=0 />
  <cc ctl=22 id=switch_10   caption="Switch 10"  class=button repeat=0 />
  <cc ctl=23 id=switch_11   caption="Switch 11"  class=button repeat=0 />
  <cc ctl=24 id=switch_12   caption="Switch 12"  class=button repeat=0 />
  <cc ctl=25 id=switch_13   caption="Switch 13"  class=button repeat=0 />
  <cc ctl=26 id=switch_14   caption="Switch 14"  class=button repeat=0 />
  <cc ctl=27 id=switch_15   caption="Switch 15"  class=button repeat=0 />
  <cc ctl=28 id=switch_16   caption="Switch 16"  class=button repeat=0 />

  <!-- Faders 9..16 -->
  <cc ctl=12 id=fader_9    caption="Fader 9"   class=value/>
  <cc ctl=13 id=fader_10   caption="Fader 10"  class=value/>
  <cc ctl=14 id=fader_11   caption="Fader 11"  class=value/>
  <cc ctl=15 id=fader_12   caption="Fader 12"  class=value/>
  <cc ctl=16 id=fader_13   caption="Fader 13"  class=value/>
