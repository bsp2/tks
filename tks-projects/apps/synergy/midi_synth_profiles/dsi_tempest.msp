      +-------+-------+-------+-------+-------+-------+-------+-------+

-->

<MIDISynthProfile
  id="dsi_tempest"
  caption="DSI Tempest"
  inherit=0
  tsl="syntheditor_generic.tsl"
  dataClass="MIDISynthProfileDataGeneric"
  formClass="EditMIDISynthFormGeneric"
  syxPatchEnc=8To7
  syxPatchEncSize=5923
  syxPatchSize=1790
  noteNameProfile="DSI Tempest (custom)"
  >

  <!-- pad bank A: mappable soft keys -->
  <note  nr=48  id=nt_c4  caption="Pad A1.1 BD1"  class=button repeat=0 norelease=1/>
