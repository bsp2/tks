<xfm align=expand>
  <Panel dir=TOP align=expand padding=0;2;0;2>

    <TabSwitch id=ts_groups dir=CENTER align=expand
               name="Tabs"
               font=big6
               fontActiveTab=big5
               keepParent=1
               cacheLayout=1
               mouseWheel=0
               >

      <!-- 0: Patch -->
      <View caption="Patch">
        <Panel align=expand><Panel align=expand padLeft=10>
            <SizeGroupX id=sgx_patch_lbc1/>
            <SizeGroupX id=sgx_patch_nav/>
            <Panel dir=TOP align=expandx padding=16;8;0;8>

              <TitledPanel dir=BOTTOM align=expandx caption="General">
                <Panel dir=BOTTOM>
                  <CheckBox id=cb_patch_autosend
                            name="AutoSend"
                            dir=LEFT
                            caption="Autosend"
                            toolTipCaption="If checked, auto-send patch to synth patch 0 when patch was edited\n\n(note) Patches 0..199 can be edited, patches 200..999 are ROM patches\n\n(note) Manual send is triggered via lctrl-s"
                            font=big7
                            iconPrefix=chb2x
                            padLeft=12
                            />

                  <FloatParam id=fp_patch_nr dir=LEFT value=0 min=0 max=999 rangeEditable=0
                              name="PatchNr"
                              cols=3
                              stepFine=1 stepCoarse=1
                              precision=0
                              textAlign=RIGHT
                              align=centerY
                              font=big6
                              toolTipCaption="Patch Nr\n\n(note) 0 is used as edit buffer"
                              />

                  <Label dir=LEFT caption="Patch Nr:"
                         padLeft=16
                         textPlacement=RIGHT
                         font=big8
                         align=centerY
                         fillBG=0
                         />

                  <TextField id=tf_patch_name dir=LEFT
                             name="Name"
                             cols=16
                             font=big6
                             align=centerY
                             toolTipCaption="Patch name"
                             />

                  <Label dir=LEFT caption="Name:"
                         textPlacement=RIGHT
                         font=big8
                         align=centerY
                         fillBG=0
                         sizeGroup=sgx_patch_lbc1
                         />
                </Panel>

                <Panel dir=BOTTOM>
                  <Button id=bt_patch_catnext dir=LEFT caption=""
                          name="Next Cat"
                          innerPadLeft=4
                          innerPadRight=4
                          padLeft=4
                          iconPlacement=RIGHT
                          icon=arr_r
                          align=CENTERY
                          toolTipCaption="Go to next patch by category"
                          repeat=1
                          sizeGroup=sgx_patch_nav
                          labelButton=hidpi
                          />

                  <Button id=bt_patch_catprev dir=LEFT caption=""
                          name="Prev Cat"
                          innerPadLeft=4
                          innerPadRight=4
                          padLeft=12
                          icon=arr_l
                          align=CENTERY
                          toolTipCaption="Go to previous patch by category"
                          repeat=1
                          sizeGroup=sgx_patch_nav
                          labelButton=hidpi
                          />

                  <ComboBox id=cm_patch_category dir=LEFT
                            name="Category"
                            align=centerY
                            options="None;Bass;Lead;Keys;Pad;Stab;Bell;Organ;Drone;FX;Bassdrum;Tom;Snaredrum;Clap;Hihat;Percussion;Other"
                            font=big7
                            />

                  <Label dir=LEFT caption="Category:"
                         textPlacement=RIGHT
                         font=big8
                         align=centerY
                         fillBG=0
                         sizeGroup=sgx_patch_lbc1
                         />
                </Panel>

                <Panel dir=BOTTOM>
                  <ComboBox id=cm_patch_voicemode dir=LEFT
                            name="VoiceMode"
                            align=centerY
                            options="Reassign;Rotate;Unison;Round Robin"
                            font=big7
                            />

                  <Label dir=LEFT caption="Voice Mode:"
                         textPlacement=RIGHT
                         align=centerY
                         fillBG=0
                         font=big8
                         sizeGroup=sgx_patch_lbc1
                         />
                </Panel>

                <Panel dir=BOTTOM>
                  <CheckBox id=cb_patch_midi_follow
                            name="Follow"
                            dir=LEFT
                            caption="Follow"
                            toolTipCaption="If checked, auto-select patch on synth when patch nr (see above) is changed\n\n(note) No-Op when Autosend is enabled (selects patch 0 as edit buffer)"
                            font=big7
                            iconPrefix=chb2x
                            padLeft=4
                            />

                  <FloatParam id=fp_patch_nr_midi dir=LEFT value=0 min=0 max=999 rangeEditable=0
                              name="MIDIPatchNr"
                              cols=3
                              stepFine=1 stepCoarse=1
                              precision=0
                              textAlign=RIGHT
                              align=centerY
                              font=big6
                              toolTipCaption="Current MIDI Patch Nr\n\n(note) MUST match the patch nr selected on the synth\n\n(note) Better don't change patches manually and let the editor handle this instead"
                              />

                  <Label dir=LEFT caption="MIDI Patch Nr:"
                         textPlacement=RIGHT
                         font=big8
                         align=centerY
                         fillBG=0
                         sizeGroup=sgx_patch_lbc1
                         />
                </Panel>

              </TitledPanel>

              <TitledPanel dir=BOTTOM align=expandx caption="Utils: Send / Receive Patch Range">

                <Button id=bt_patch_receive
                        dir=LEFT caption=" Receive "
                        textPlacement=RIGHT
                        align=centerY
                        fillBG=0
                        font=big8
                        toolTipCaption="Dump requested patch range from synth"
                        padLeft=12
                        style=hidpi?flat
                        />

                <Button id=bt_patch_send
                        dir=LEFT caption=" Send "
                        textPlacement=RIGHT
                        align=centerY
                        fillBG=0
                        font=big8
                        toolTipCaption="Send requested patch range to synth"
                        padLeft=12
                        style=hidpi?flat
                        />

                <FloatParam id=fp_patch_last dir=LEFT value=0 min=0 max=999 rangeEditable=0
                            name="Last"
                            cols=2
                            stepFine=1 stepCoarse=1
                            precision=0
                            textAlign=RIGHT
                            align=centerY
                            toolTipCaption="Last patch nr"
                            font=big7
                            />

                <Label dir=LEFT caption="Last:"
                       textPlacement=RIGHT
                       align=centerY
                       fillBG=0
                       font=big8
                       padLeft=12
                       />

                <FloatParam id=fp_patch_first dir=LEFT value=0 min=0 max=999 rangeEditable=0
                            name="First"
                            cols=2
                            stepFine=1 stepCoarse=1
                            precision=0
                            textAlign=RIGHT
                            align=centerY
                            toolTipCaption="First patch nr"
                            font=big7
                            />

                <Label dir=LEFT caption="First:"
                       textPlacement=RIGHT
                       align=centerY
                       fillBG=0
                       font=big8
                       sizeGroup=sgx_patch_lbc1
                       />
              </TitledPanel>


              <TitledPanel dir=BOTTOM align=expandx caption="Utils: Copy / Paste / Init">

                <Button id=bt_patch_saveinit
                        dir=RIGHT caption=" Save as Init "
                        textPlacement=RIGHT
                        align=centerY
                        fillBG=0
                        font=big8
                        toolTipCaption="Save current patch as init patch"
                        padLeft=12
                        style=hidpi?flat
                        />

                <Button id=bt_patch_init
                        dir=LEFT caption=" Init "
                        textPlacement=RIGHT
                        align=centerY
                        fillBG=0
                        font=big8
                        toolTipCaption="Init current patch"
                        padLeft=12
                        style=hidpi?flat
                        />

                <Button id=bt_patch_paste
                        dir=LEFT caption=" Paste "
                        textPlacement=RIGHT
                        align=centerY
                        fillBG=0
                        font=big8
                        toolTipCaption="Paste clipboard to current patch"
                        padLeft=12
                        style=hidpi?flat
                        />

                <Button id=bt_patch_copy
                        dir=LEFT caption=" Copy "
                        textPlacement=RIGHT
                        align=centerY
                        fillBG=0
                        font=big8
                        toolTipCaption="Copy current patch to clipboard"
                        style=hidpi?flat
                        />

                <Label dir=LEFT caption=" "
                       textPlacement=RIGHT
                       align=centerY
                       fillBG=0
                       font=big8
                       sizeGroup=sgx_patch_lbc1
                       />
              </TitledPanel>

            </Panel>

        </Panel></Panel>
      </View>


      <!-- 1: DCO -->
      <View caption="DCO">
        <Panel align=expand><Panel align=expand padLeft=10>
            <SizeGroupX id=sgx_dco_lbc1/>
            <SizeGroupX id=sgx_dco_lbc2/>
            <Panel dir=TOP align=expandx padding=8;8;0;8>

              <!-- Mix / Detune -->
              <TitledPanel caption="Mix / Detune" dir=BOTTOM align=expandx padTop=0>
                <ComboBox id=cm_dco_sync dir=LEFT
                          name="DCOSync"
                          align=centerY
                          options="Off;Soft Sync;Medium Sync;Hard Sync"
                          toolTipCaption="DCO Sync Mode\n\nSoft Sync: Clock signal for DCO1 is the result of OR\'ing DCO1 and DCO2 clock signals\n\nMedium Sync: Combined Soft+Hard Sync\n\nHard Sync: Reset DCO1 clock each time DCO2 reaches a whole phase"
                          font=big7
                          />

                <Label dir=LEFT caption="Sync DCO1 to DCO2:"
                       padLeft=16
                       textPlacement=RIGHT
                       align=centerY
                       fillBG=0
                       font=big8
                       />

                <FloatParam id=fp_dco2_detune dir=LEFT value=0 min=-32 max=31 rangeEditable=0
                            name="DCO2Detune"
                            cols=2
                            stepFine=1 stepCoarse=1
                            precision=0
                            textAlign=RIGHT
                            align=centerY
                            toolTipCaption="DCO2 detune"
                            font=big7
                            />

                <Label dir=LEFT caption="DCO2 Detune:"
                       padLeft=16
                       textPlacement=RIGHT
                       align=centerY
                       fillBG=0
                       font=big8
                       />

                <FloatParam id=fp_dco_mix dir=LEFT value=0 min=0 max=63 rangeEditable=0
                            name="DCOMix"
                            cols=2
                            stepFine=1 stepCoarse=1
                            precision=0
                            textAlign=RIGHT
                            align=centerY
                            font=big7
                            toolTipCaption="DCO Mix (63=DCO1, 0=DCO2)"
                            />
                <Label dir=LEFT caption="DCO2/1 Mix:"
                       padLeft=16
                       textPlacement=RIGHT
                       align=centerY
                       fillBG=0
                       font=big8
                       sizeGroup=sgx_dco_lbc1
                       />
              </TitledPanel>

              <!-- DCO 1 -->
              <TitledPanel caption="DCO 1" align=expandx dir=BOTTOM>

                <Panel dir=BOTTOM>

                  <!-- Click / Shape -->
                  <Panel dir=RIGHT>
                    <Panel dir=TOP>
                      <CheckBox id=cb_dco1_click
                                name="EnClick"
                                dir=LEFT
                                caption="Click"
                                toolTipCaption="If checked, enable click"
                                font=big7
                                iconPrefix=chb2x
                                />

                      <Label dir=LEFT caption=" "
                             textPlacement=RIGHT
                             align=centerY
                             fillBG=0
                             sizeGroup=sgx_dco_lbc1
                             font=big8
                             />
                    </Panel>

                    <Panel dir=BOTTOM padTop=6>
                      <CheckBox id=cb_dco1_sawtri
                                name="EnSawTri"
                                dir=LEFT
                                caption=""
                                toolTipCaption="If checked, enable sawtooth/triangle waveform"
                                padLeft=4
                                iconPrefix=chb2x
                                font=big7
                                />

                      <FloatParam id=fp_dco1_shape dir=LEFT value=0 min=0 max=63 rangeEditable=0
                                  name="DCO1Shape"
                                  cols=2
                                  stepFine=1 stepCoarse=1
                                  precision=0
                                  textAlign=RIGHT
                                  align=centerY
                                  toolTipCaption="DCO1 initial waveshape (0=Sawtooth, 63=Triangle)"
                                  font=big7
                                  />

                      <Label dir=LEFT caption="Saw/Tri Shape:"
                             textPlacement=RIGHT
                             align=centerY
                             fillBG=0
                             font=big8
                             sizeGroup=sgx_dco_lbc1
                             />
                    </Panel>
                  </Panel>

                  <!-- Frequency / Pulse-Width -->
                  <Panel dir=RIGHT padLeft=16>
                    <Panel dir=TOP>
                      <FloatParam id=fp_dco1_freq dir=LEFT value=0 min=0 max=63 rangeEditable=0
                                  name="DCO1Freq"
                                  cols=2
                                  stepFine=1 stepCoarse=1
                                  precision=0
                                  textAlign=RIGHT
                                  align=centerY
                                  toolTipCaption="DCO1 initial frequency"
                                  font=big7
                                  />

                      <Label dir=LEFT caption="Frequency:"
                             textPlacement=RIGHT
                             align=centerY
                             fillBG=0
                             font=big8
                             sizeGroup=sgx_dco_lbc2
                             />
                    </Panel>

                    <Panel dir=BOTTOM padTop=6>
                      <CheckBox id=cb_dco1_pulse
                                name="EnPulse"
                                dir=LEFT
                                caption=""
                                toolTipCaption="If checked, enable pulse waveform"
                                padLeft=4
                                font=big7
                                iconPrefix=chb2x
                                />

                      <FloatParam id=fp_dco1_pw dir=LEFT value=0 min=0 max=63 rangeEditable=0
                                  name="DCO1PW"
                                  cols=2
                                  stepFine=1 stepCoarse=1
                                  precision=0
                                  textAlign=RIGHT
                                  align=centerY
                                  toolTipCaption="DCO1 initial pulse width"
                                  font=big7
                                  />

                      <Label dir=LEFT caption="Pulse-Width:"
                             textPlacement=RIGHT
                             align=centerY
                             fillBG=0
                             font=big8
                             sizeGroup=sgx_dco_lbc2
                             />
                    </Panel>
                  </Panel>

                  <!-- LFO1/2 Amount -->
                  <Panel dir=RIGHT padLeft=16>
                    <Panel dir=TOP>
                      <FloatParam id=fp_dco1_lfo1amt dir=LEFT value=0 min=-63 max=63 rangeEditable=0
                                  name="DCO1LFO1"
                                  cols=2
                                  stepFine=1 stepCoarse=1
                                  precision=0
                                  textAlign=RIGHT
                                  align=centerY
                                  toolTipCaption="DCO1 LFO1 to Freq Amount"
                                  font=big7
                                  />

                      <Label dir=LEFT caption="LFO1 Amount:"
                             textPlacement=RIGHT
                             align=centerY
                             fillBG=0
                             padLeft=16
                             font=big8
                             />
                    </Panel>

                    <Panel dir=BOTTOM padTop=6>
                      <FloatParam id=fp_dco1_lfo2amt dir=LEFT value=0 min=-63 max=63 rangeEditable=0
                                  name="DCO1LFO2"
                                  cols=2
                                  stepFine=1 stepCoarse=1
                                  precision=0
                                  textAlign=RIGHT
                                  align=centerY
                                  toolTipCaption="DCO1 LFO1 to PulseWidth Amount"
                                  font=big7
                                  />

                      <Label dir=LEFT caption="LFO2 Amount:"
                             textPlacement=RIGHT
                             align=centerY
                             fillBG=0
                             padLeft=16
                             font=big8
                             />
                    </Panel>
                  </Panel>

                </Panel>

                <Panel dir=BOTTOM padTop=6>
                  <CheckBox id=cb_dco1_portamento
                            name="DCO1FixPortamento"
                            dir=LEFT
                            caption="Portamento"
                            toolTipCaption="If checked, enable Portamento fixed modulation"
                            padLeft=4
                            font=big7
                            iconPrefix=chb2x
                            />

                  <CheckBox id=cb_dco1_vibrato
                            name="DCO1FixVibrato"
                            dir=LEFT
                            caption="Vibrato"
                            toolTipCaption="If checked, enable Vibrato fixed modulation"
                            padLeft=4
                            font=big7
                            iconPrefix=chb2x
                            />

                  <CheckBox id=cb_dco1_lever1
                            name="DCO1FixLever1"
                            dir=LEFT
                            caption="Pitchbend"
                            toolTipCaption="If checked, enable pitchbend fixed modulation"
                            font=big7
                            iconPrefix=chb2x
                            />

                  <Label dir=LEFT caption="FixMod:"
                         textPlacement=RIGHT
                         align=centerY
                         fillBG=0
                         sizeGroup=sgx_dco_lbc1
                         font=big8
                         />
                </Panel>

              </TitledPanel>


              <!-- DCO 2 -->
              <TitledPanel caption="DCO 2" align=expandx dir=BOTTOM>

                <Panel dir=BOTTOM>

                  <!-- Click / Shape -->
                  <Panel dir=RIGHT>
                    <Panel dir=TOP>
                      <CheckBox id=cb_dco2_click
                                name="EnClick"
                                dir=LEFT
                                caption="Click"
                                toolTipCaption="If checked, enable click"
                                font=big7
                                iconPrefix=chb2x
                                />

                      <Label dir=LEFT caption=" "
                             textPlacement=RIGHT
                             align=centerY
                             fillBG=0
                             sizeGroup=sgx_dco_lbc1
                             font=big8
                             />
                    </Panel>

                    <Panel dir=BOTTOM padTop=6>
                      <CheckBox id=cb_dco2_sawtri
                                name="EnSawTri"
                                dir=LEFT
                                caption=""
                                toolTipCaption="If checked, enable sawtooth/triangle waveform"
                                padLeft=4
                                iconPrefix=chb2x
                                font=big7
                                />

                      <FloatParam id=fp_dco2_shape dir=LEFT value=0 min=0 max=63 rangeEditable=0
                                  name="DCO2Shape"
                                  cols=2
                                  stepFine=1 stepCoarse=1
                                  precision=0
                                  textAlign=RIGHT
                                  align=centerY
                                  toolTipCaption="DCO2 initial waveshape (0=Sawtooth, 63=Triangle)"
                                  font=big7
                                  />

                      <Label dir=LEFT caption="Saw/Tri Shape:"
                             textPlacement=RIGHT
                             align=centerY
                             fillBG=0
                             font=big8
                             sizeGroup=sgx_dco_lbc1
                             />
                    </Panel>
                  </Panel>

                  <!-- Frequency / Pulse-Width -->
                  <Panel dir=RIGHT padLeft=16>
                    <Panel dir=TOP>
                      <FloatParam id=fp_dco2_freq dir=LEFT value=0 min=0 max=63 rangeEditable=0
                                  name="DCO2Freq"
                                  cols=2
                                  stepFine=1 stepCoarse=1
                                  precision=0
                                  textAlign=RIGHT
                                  align=centerY
                                  toolTipCaption="DCO2 initial frequency"
                                  font=big7
                                  />

                      <Label dir=LEFT caption="Frequency:"
                             textPlacement=RIGHT
                             align=centerY
                             fillBG=0
                             font=big8
                             sizeGroup=sgx_dco_lbc2
                             />
                    </Panel>

                    <Panel dir=BOTTOM padTop=6>
                      <CheckBox id=cb_dco2_pulse
                                name="EnPulse"
                                dir=LEFT
                                caption=""
                                toolTipCaption="If checked, enable pulse waveform"
                                padLeft=4
                                font=big7
                                iconPrefix=chb2x
                                />

                      <FloatParam id=fp_dco2_pw dir=LEFT value=0 min=0 max=63 rangeEditable=0
                                  name="DCO2PW"
                                  cols=2
                                  stepFine=1 stepCoarse=1
                                  precision=0
                                  textAlign=RIGHT
                                  align=centerY
                                  toolTipCaption="DCO2 initial pulse width"
                                  font=big7
                                  />

                      <Label dir=LEFT caption="Pulse-Width:"
                             textPlacement=RIGHT
                             align=centerY
                             fillBG=0
                             font=big8
                             sizeGroup=sgx_dco_lbc2
                             />
                    </Panel>
                  </Panel>

                  <!-- LFO1/2 Amount -->
                  <Panel dir=RIGHT padLeft=16>
                    <Panel dir=TOP>
                      <FloatParam id=fp_dco2_lfo1amt dir=LEFT value=0 min=-63 max=63 rangeEditable=0
                                  name="DCO2LFO1"
                                  cols=2
                                  stepFine=1 stepCoarse=1
                                  precision=0
                                  textAlign=RIGHT
                                  align=centerY
                                  toolTipCaption="DCO2 LFO1 to Freq Amount"
                                  font=big7
                                  />

                      <Label dir=LEFT caption="LFO1 Amount:"
                             textPlacement=RIGHT
                             align=centerY
                             fillBG=0
                             padLeft=16
                             font=big8
                             />
                    </Panel>

                    <Panel dir=BOTTOM padTop=6>
                      <CheckBox id=cb_dco2_noise
                                name="DCO2Noise"
                                dir=LEFT
                                caption="Noise"
                                toolTipCaption="If checked, enable noise generator"
                                padLeft=16
                                font=big7
                                iconPrefix=chb2x
                                />

                      <FloatParam id=fp_dco2_lfo2amt dir=LEFT value=0 min=-63 max=63 rangeEditable=0
                                  name="DCO2LFO2"
                                  cols=2
                                  stepFine=1 stepCoarse=1
                                  precision=0
                                  textAlign=RIGHT
                                  align=centerY
                                  toolTipCaption="DCO2 LFO1 to PulseWidth Amount"
                                  font=big7
                                  />

                      <Label dir=LEFT caption="LFO2 Amount:"
                             textPlacement=RIGHT
                             align=centerY
                             fillBG=0
                             padLeft=16
                             font=big8
                             />
                    </Panel>
                  </Panel>

                </Panel>

                <Panel dir=BOTTOM padTop=6>
                  <CheckBox id=cb_dco2_keytrack
                            name="DCO2FixKeyTrack"
                            dir=LEFT
                            caption="KeyTrack"
                            toolTipCaption="If checked, enable KeyTrack fixed modulation"
                            padLeft=4
                            font=big7
                            iconPrefix=chb2x
                            />

                  <CheckBox id=cb_dco2_portamento
                            name="DCO2FixPortamento"
                            dir=LEFT
                            caption="Portamento"
                            toolTipCaption="If checked, enable Portamento fixed modulation"
                            padLeft=4
                            font=big7
                            iconPrefix=chb2x
                            />

                  <CheckBox id=cb_dco2_vibrato
                            name="DCO2FixVibrato"
                            dir=LEFT
                            caption="Vibrato"
                            toolTipCaption="If checked, enable Vibrato fixed modulation"
                            padLeft=4
                            font=big7
                            iconPrefix=chb2x
                            />

                  <CheckBox id=cb_dco2_lever1
                            name="DCO2FixLever1"
                            dir=LEFT
                            caption="Pitchbend"
                            toolTipCaption="If checked, enable pitchbend fixed modulation"
                            iconPrefix=chb2x
                            font=big7
                            />

                  <Label dir=LEFT caption="FixMod:"
                         textPlacement=RIGHT
                         align=centerY
                         fillBG=0
                         sizeGroup=sgx_dco_lbc1
                         font=big8
                         />
                </Panel>

              </TitledPanel>


            </Panel>
        </Panel></Panel>
      </View>

      <!-- 2: Env -->
      <View caption="Env">
        <Panel align=expand><Panel align=expand padLeft=10>
            <SizeGroupX id=sgx_env_lbc1/>
            <ScrollPane dir=TOP align=expandx adaptiveViewSize=1>

              <Panel><Panel dir=TOP align=expandx >
                  <TitledPanel dir=BOTTOM caption="Envelope 1 (Filter)" align=expandx>

                    <Panel dir=BOTTOM align=expandx>

                      <Panel dir=RIGHT align=centerY padRight=8>
                        <FloatParam id=fp_env1_fmenv3 dir=LEFT value=0 min=-63 max=63 rangeEditable=0
                                    name="Env1FMEnv3"
                                    cols=1
                                    stepFine=1 stepCoarse=1
                                    precision=0
                                    textAlign=RIGHT
                                    align=centerY
                                    font=big7
                                    toolTipCaption="Filter FM envelope 3 amount"
                                    />

                        <Label dir=LEFT caption="Env3:"
                               textPlacement=RIGHT
                               align=centerY
                               fillBG=0
                               font=big8
                               padLeft=12
                               />

                        <FloatParam id=fp_env1_fmpressure dir=LEFT value=0 min=-63 max=63 rangeEditable=0
                                    name="Env1FMPress"
                                    cols=1
                                    stepFine=1 stepCoarse=1
                                    precision=0
                                    textAlign=RIGHT
                                    align=centerY
                                    font=big7
                                    toolTipCaption="Filter FM pressure FM amount"
                                    />

                        <Label dir=LEFT caption="Press:"
                               textPlacement=RIGHT
                               align=centerY
                               fillBG=0
                               font=big8
                               padLeft=12
                               />

                        <FloatParam id=fp_env1_fm dir=LEFT value=0 min=0 max=63 rangeEditable=0
                                    name="Env1FM"
                                    cols=1
                                    stepFine=1 stepCoarse=1
                                    precision=0
                                    textAlign=RIGHT
                                    align=centerY
                                    font=big7
                                    toolTipCaption="Filter DCO1=>CutOff FM amount"
                                    />

                        <Label dir=LEFT caption="FM:"
                               textPlacement=RIGHT
                               align=centerY
                               fillBG=0
                               font=big8
                               />
                      </Panel>

                      <FloatParam id=fp_env1_res dir=LEFT value=0 min=0 max=63 rangeEditable=0
                                  name="Env1Res"
                                  cols=1
