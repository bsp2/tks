function Process(MIDIPipeFrame frame) {
  MIDIPipeFrame frClone = frame;
  frClone.replaceDevIdxAndMidiCh(in_1_dev, in_1_ch, out_1_dev, out_1_ch);
  frame.mergeFrame(frClone);
}

function OnProcess(MIDIPipeFrame framePlay,
                   MIDIPipeFrame frameRec,
                   boolean       bMuted,
                   boolean       bPlaySeq
                   ) {
   if(!bMuted)
   {
      Process(frameRec);
      Process(framePlay);  
   }
}
