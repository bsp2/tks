         MontagePartFMOperator op <= ops.get(opIdx);
         MontagePartFMOperator oop <= _o.ops.get(opIdx);
         // trace "xxx copy opIdx="+opIdx+" controller set";
         op.copyControllerSet(oop);
         op.copyPitchLevelControllerSensitivity(oop);
         opIdx++;
      }
