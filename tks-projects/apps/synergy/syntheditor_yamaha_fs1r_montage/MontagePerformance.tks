                 $1E, 1, G_LFO,    // User LFO Step Value 9
                 $1F, 1, G_LFO,    // User LFO Step Value 10
                 $20, 1, G_LFO,    // User LFO Step Value 11
                 $21, 1, G_LFO,    // User LFO Step Value 12
