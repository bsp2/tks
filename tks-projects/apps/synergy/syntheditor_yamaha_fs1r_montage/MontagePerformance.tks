   public method dumpEGSettings(PointerArray _currentEGSettings) {
      Montage_EGPreset *p;

      p <= new Montage_EGPreset;
      p.name = "FM peg";
      savePEGPreset(p);
      _currentEGSettings.add(#(deref p));
      p.dump();

      p <= new Montage_EGPreset;
      p.name = "FM feg";
      saveFEGPreset(p);
      _currentEGSettings.add(#(deref p));
      p.dump();
   }

