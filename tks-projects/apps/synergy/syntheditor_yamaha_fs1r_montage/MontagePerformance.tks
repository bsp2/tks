                 $09, 1, G_SOUND,   // Spectral Form
                 $0A, 1, G_SOUND,   // Spectral Skirt
                 $0B, 1, G_SOUND,   // Spectral Resonance
                 $0C, 1, G_SOUND,   // PEG Initial Level
                 $0D, 1, G_SOUND,   // PEG Attack Level
                 $0E, 1, G_SOUND,   // PEG Attack Time
                 $0F, 1, G_SOUND,   // PEG Decay Time
