   public virtual saveState(Stream ofs) {
      ofs.i16 = 1; // ver

      ofs.i8 = element_valid_mask;
      MontagePartNormalElement *e;
      int bit = 1;
      int elementIdx = 0;
      foreach e in elements
      {
