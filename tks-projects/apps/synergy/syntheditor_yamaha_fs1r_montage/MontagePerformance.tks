   public method setControllerSetBox(byte idx, byte _v) {
      // idx: 0..15   v: 0,1
      byte addr;
      if(idx >= 14)
      {
         addr = $27;
         idx -= 14;
      }
      else if(idx >= 7)
      {
         addr = $28;
         idx -= 7;
      }
      else
      {
         addr = $29;
      }
      _v = (data.peekI8(addr) & ~(1 << idx)) | ((_v&1) << idx);
      data.pokeI8(addr, _v);
   }

   public method getControllerSetBox(byte idx) : byte {
      // if(0 == idx)
      // {
      //    trace "xxx getControllerSetBox(idx="+idx+")";
      //    data.hexdump($25, 5);
      // }
      byte addr;
      if(idx >= 14)
      {
         addr = $27;
         idx -= 14;
      }
      else if(idx >= 7)
      {
         addr = $28;
         idx -= 7;
      }
      else
      {
         addr = $29;
      }
      return (0 != (data.peekI8(addr) & (1 << idx)));
   }

