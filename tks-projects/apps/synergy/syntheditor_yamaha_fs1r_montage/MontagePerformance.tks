   public method setControllerSetBox(byte idx, byte _v) {
      // idx: 0..15   v: 0,1
      byte addr;
      if(idx >= 14)
      {
         addr = $1B;
         idx -= 14;
      }
      else if(idx >= 7)
      {
         addr = $1C;
         idx -= 7;
      }
      else
      {
         addr = $1D;
      }
      _v = (data.peekI8(addr) & ~(1 << idx)) | ((_v&1) << idx);
      data.pokeI8(addr, _v);
   }

   public method getControllerSetBox(byte idx) : byte {
      byte addr;
      if(idx >= 14)
      {
         addr = $1B;
         idx -= 14;
      }
      else if(idx >= 7)
      {
         addr = $1C;
         idx -= 7;
      }
      else
      {
         addr = $1D;
      }
      return (0 != (data.peekI8(addr) & (1 << idx)));
   }

