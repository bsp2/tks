   public method setName(String _s) {
      int numChars = mathMini(20, _s.numChars);
      int idx = 0;
      loop(numChars)
      {
         data.pokeI8(idx, _s.getc(idx));
         idx++;
      }
      loop(20 - numChars)
         data.pokeI8(idx++, ' ');
   }

   public method getName() : String {
      local String r;
      r.alloc(20+1);
      int idx = 0;
      // trace "xxx getName. data=";
      // data.hexdump(0, data.offset);
      loop(20)
      {
         r[idx] = data.peekI8(idx);
         idx++;
      }
      r.putc(idx, 0);
      r.fixLength();
      return deref r;
   }

