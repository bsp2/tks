            e.saveState(ofs);
         bit = bit << 1;
         elementIdx++;
      }
   }

   // <load.png>
   public virtual loadState(Stream ifs) : boolean {
      short ver = ifs.u16;
      if(ver >= 1)
      {
         element_valid_mask = ifs.u8;

         MontagePartNormalElement *e;
         int bit = 1;
         int elementIdx = 0;
         foreach e in elements
         {
            if(element_valid_mask & bit)
            {
               if(!e.loadState(ifs))
                  return false;
            }
            bit = bit << 1;
            elementIdx++;
         }
         return true;
      }
      else
      {
         trace "[---] "+yacMetaClassName()+"::loadState: invalid ver="+ver;
      }
      return false;
   }

}


