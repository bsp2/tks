      part_idx = _partIdx;
      allocData(39, // $27
                [$00, 1, G_LFO,    // LFO Phase
                 $01, 1, G_LFO,    // LFO Wave
                 $02, 1, G_LFO,    // LFO Speed
                 $03, 1, G_LFO,    // LFO Tempo Speed
