            sendData($30, $4E, $00, _outDev);

            if(_bHeader)
               sendBulkFooter(_outDev);

            copyToReplay(_replayAudioInPartAD);
         }

      }

      if(0 == (g_fx_mode & 1))
      {
         setInsertionFXSwitch(origSW);
      }

      return _numDiffsLeft;
   }

