   // <method.png>
   public =replay= method pasteOpVClipboard(int _opIdx) : boolean {
      if(null != voiced_op_clipboard)
      {
         MontagePerformance perf <= patch.getEditPerformance(data.current_part_idx);
         if(null != perf)
         {
            MontagePartFM partFM <= getEditPartFM();
