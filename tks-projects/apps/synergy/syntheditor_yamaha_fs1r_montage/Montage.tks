   // <method.png>
   protected static CalcMuteString(String _s, int _mask) {
      int bit = 1;
      int nr = 1;
      loop(8)
      {
         if(_mask & bit)
            _s.append("-");
         else
            _s.append(String(nr));
         bit = (bit << 1);
         nr++;
      }
   }

