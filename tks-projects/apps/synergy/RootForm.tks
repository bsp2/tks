   // <ui_kbd.png>
   public virtual onTriadKey(Key _k) : boolean {
      // // trace "xxx RootForm: onTriadKey: _k.code="+_k.code;
      int subSongIdx = -1;

      switch(_k.pressed)
      {
         case VKEY_ESCAPE:
            showTransportStopMenu(true/*bFocusFirst*/);
            return true;

         case VKEY_KP0:
            subSongIdx = 0;
            break;

         case VKEY_KP1:
            subSongIdx = 1;
            break;

         case VKEY_KP2:
            subSongIdx = 2;
            break;

         case VKEY_KP3:
            subSongIdx = 3;
            break;

         case VKEY_KP4:
            subSongIdx = 4;
            break;

         case VKEY_KP5:
            subSongIdx = 5;
            break;

         case VKEY_KP6:
            subSongIdx = 6;
            break;

         case VKEY_KP7:
            subSongIdx = 7;
            break;

         case VKEY_KP8:
            subSongIdx = 8;
            break;

         case VKEY_KP9:
            subSongIdx = 9;
            break;

         case VKEY_KP_PLUS:
            showBPMDialog();
            break;

         case VKEY_KP_ENTER:
         case VKEY_EQUALS:
            sendDAWUndoMessage();
            break;

         case 'x':
            if(!SysEx.IsAudioConnected())
               startAudioProcess();
            showEurekaWindow(true/*bFocus*/);
            break;
      }

      if(-1 != subSongIdx)
      {
         if(STConfig.b_lctrlx_kp_select_multimorph)
         {
            selectMultiSceneByIdx(subSongIdx);
         }
         else
         {
            if(!isCurrentPageModal())
            {
               selectSubSongByIdxAndShowPipeMap(subSongIdx);
            }

            UI.RedrawAll();
         }
      }
   }

