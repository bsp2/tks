<xfm align=expand>
  <Panel dir=CENTER align=expand padding=0;0;0;0 alpha=255>

    <TabSwitch id=ts_groups
               name="Tabs"
               dir=CENTER
               align=expand
               fontActiveTab=big7
               keepParent=1
               cacheLayout=1
               mouseWheel=0
               >

      <!-- 0: Pattern -->
      <View caption="Pattern">
        <Panel align=expand><Panel align=expand padLeft=10>

            <SizeGroupX id=sgx_pat_lbc1/>
            <SizeGroupX id=sgx_pat_cmc1/>
            <SizeGroupX id=sgx_pat_lbc2/>
            <SizeGroupX id=sgx_pat_cmc2/>
            <SizeGroupX id=sgx_pat_lbc3/>
            <SizeGroupX id=sgx_pat_cmc3/>

            <!-- Pattern alloc/free -->
            <Panel dir=TOP align=expandx padding=16;8;0;8>
              <SizeGroupX id=sgx_bt_pat/>

              <Button id=bt_pat_tools
                      name="Pat.Tools.."
                      dir=CENTER
                      align=centerY
                      caption="Tools.."
                      font=big5
                      icon=tools
                      iconMono=tools_mono
                      labelButton=1
                      toolTipCaption="Show pattern tools menu"
                      innerPadLeft=30
                      innerPadRight=30
                      sizeGroup=sgx_bt_pat
                      />

              <Button id=bt_pat_alloc
                      name="Pat.Alloc"
                      dir=LEFT
                      align=centerY
                      caption="New"
                      font=big5
                      icon=add
                      iconMono=add_mono
                      toolTipCaption="Allocate new pattern.\n\n (note) If the currently selected pattern is unused, it will be allocated.\n\n (note) If the currently selected pattern is in use, the next unused pattern will be allocated.\n\n (note) Hold-click to clone pattern"
                      labelButton=1
                      hold=1
                      innerPadLeft=30
                      innerPadRight=30
                      sizeGroup=sgx_bt_pat
                      />

              <Button id=bt_pat_free
                      name="Pat.Free"
                      dir=RIGHT
                      align=centerY
                      caption="Free"
                      font=big5
                      icon=remove
                      iconMono=remove_mono
                      toolTipCaption="Free pattern."
                      labelButton=1
                      innerPadLeft=30
                      innerPadRight=30
                      sizeGroup=sgx_bt_pat
                      />
            </Panel>

            <Panel dir=TOP align=expandx padTop=16 padBottom=0>

              <TextField id=tf_pat_name
                         name="Name"
                         dir=LEFT
                         align=centerY
                         font=big6
                         cols=16
                         toolTipCaption="Pattern name\n\n (note) lshift-n"
                         />

              <Label dir=LEFT
                     align=centerY
                     fillBG=0
                     caption="Name:"
                     font=big7
                     textPlacement=RIGHT
                     padLeft=16
                     sizeGroup=sgx_pat_lbc2
                     />

              <ComboBox id=cm_pattern_nr
                        name="PatNr"
                        dir=LEFT
                        align=centerY
                        font=big4
                        toolTipCaption="Current pattern"
                        reverseWheelDir=1
                        largeArrow=1
                        sizeGroup=sgx_pat_cmc1
                        />

              <Label dir=LEFT
                     align=centerY
                     caption="Pattern:\n  (127)"
                     id=lb_pat_nr
                     font=big7
                     fillBG=1
                     textPlacement=RIGHT
                     sizeGroup=sgx_pat_lbc1
                     />

            </Panel>

            <Panel></Panel> <!-- grab all available space -->

            <TitledPanel dir=BOTTOM align=expandx caption="Preset">

              <Button id=bt_preset_save
                      name="PstSave"
                      dir=LEFT
                      align=centerY
                      caption="Save.."
                      font=big7
                      toolTipCaption="Save preset"
                      labelButton=1
                      padLeft=15
                      />

              <Button id=bt_preset_load
                      name="PstLoad"
                      dir=LEFT
                      align=centerY
                      caption="Load"
                      font=big7
                      toolTipCaption="Load selected preset\n\n(note) Replace current pattern if it is empty, create new pattern otherwise"
                      labelButton=1
                      hold=1
                      padLeft=15
                      />


              <ComboBox id=cm_preset
                        name="Preset"
                        dir=LEFT
                        align=centerY
                        font=big7
                        toolTipCaption="Preset"
                        reverseWheelDir=1
                        largeArrow=1
                        />

            </TitledPanel>

        </Panel></Panel>
      </View>


      <!-- 1: MacroCtl -->
      <View caption="Macro Ctl">
        <Panel align=expand prefsizey=filly><Panel align=expandx dir=TOP>

            <SizeGroupX id=sgx_en_lbc1/>
            <SizeGroupX id=sgx_bt1/>
            <SizeGroupX id=sgx_bt2/>
            <SizeGroupX id=sgx_bt3/>

            <TitledPanel dir=BOTTOM caption="Macro Control 1" noBorderStyle=1 drawBorder=0 align=expandx>

              <Panel dir=BOTTOM align=expandx>

                <TextField id=tf_m1_name
                           name="M1Name"
                           dir=LEFT
                           align=centerY
                           font=big8
                           toolTipCaption="Macro Control 1 Name"
                           cols=40
                           />

                <Label id=lb_en_lbc1
                       autoresolve=0
                       dir=LEFT
                       align=centerY
                       fillBG=0
                       caption="Name:"
                       textPlacement=RIGHT
                       padLeft=5
                       sizeGroup=sgx_en_lbc1
                       />
              </Panel>

              <ViewPane dir=BOTTOM align=expandx adaptiveViewSize=1 collapse=1 forceMinSizeY=1 padLeft=5>
                <NMM_ArgForm id=af_m1 dir=CENTER align=expandx />
              </ViewPane>

            </TitledPanel>


            <TitledPanel dir=BOTTOM caption="Macro Control 2" noBorderStyle=1 drawBorder=0 align=expandx>

              <Panel dir=BOTTOM align=expandx>

                <TextField id=tf_m2_name
                           name="M2Name"
                           dir=LEFT
                           align=centerY
                           font=big8
                           toolTipCaption="Macro Control 2 Name"
                           cols=40
                           />

                <Label dir=LEFT
                       align=centerY
                       fillBG=0
                       caption="Name:"
                       textPlacement=RIGHT
                       padLeft=5
                       sizeGroup=sgx_en_lbc1
                       />
              </Panel>

              <ViewPane dir=BOTTOM align=expandx adaptiveViewSize=1 collapse=1 forceMinSizeY=1 padLeft=5>
                <NMM_ArgForm id=af_m2 dir=CENTER align=expandx/>
              </ViewPane>

            </TitledPanel>


            <TitledPanel dir=BOTTOM caption="Macro Control 3" noBorderStyle=1 drawBorder=0 align=expandx>

              <Panel dir=BOTTOM align=expandx>

                <TextField id=tf_m3_name
                           name="M3Name"
                           dir=LEFT
                           align=centerY
                           font=big8
                           toolTipCaption="Macro Control 3 Name"
                           cols=40
                           />

                <Label dir=LEFT
                       align=centerY
                       fillBG=0
                       caption="Name:"
                       textPlacement=RIGHT
                       padLeft=5
                       sizeGroup=sgx_en_lbc1
                       />
              </Panel>

              <ViewPane dir=BOTTOM align=expandx adaptiveViewSize=1 collapse=1 forceMinSizeY=1 padLeft=5>
                <NMM_ArgForm id=af_m3 dir=CENTER align=expandx/>
              </ViewPane>

            </TitledPanel>


            <TitledPanel dir=BOTTOM caption="Macro Control 4" noBorderStyle=1 drawBorder=0 align=expandx>

              <Panel dir=BOTTOM align=expandx>

                <TextField id=tf_m4_name
                           name="M4Name"
                           dir=LEFT
                           align=centerY
                           font=big8
                           toolTipCaption="Macro Control 4 Name"
                           cols=40
                           />

                <Label dir=LEFT
                       align=centerY
                       fillBG=0
                       caption="Name:"
                       textPlacement=RIGHT
                       padLeft=5
                       sizeGroup=sgx_en_lbc1
                       />
              </Panel>

              <ViewPane dir=BOTTOM align=expandx adaptiveViewSize=1 collapse=1 forceMinSizeY=1 padLeft=5>
                <NMM_ArgForm id=af_m4 dir=CENTER align=expandx/>
              </ViewPane>

            </TitledPanel>

        </Panel></Panel>
      </View>


      <!-- 2: Entries -->
      <View caption="Entries">
        <Panel align=expand prefsizey=filly><Panel align=expand>

            <!-- Node Class / Show in arranger -->
            <Panel dir=CENTER padBottom=0 align=expand>
              <TableView id=tv_entries
                         align=expand
                         style=flat
                         homeendkeys=0
                         />
            </Panel>

            <SizeGroupX id=sgx_en_lbc1/>
            <SizeGroupX id=sgx_bt1/>
            <SizeGroupX id=sgx_bt2/>
            <SizeGroupX id=sgx_bt3/>

            <!-- Insert/Delete/Move up/Move Down/Copy/Paste -->
            <Panel dir=BOTTOM padTop=2>
              <CheckBox id=cb_show_all_args
                        name="ShowArgs"
                        dir=LEFT
                        caption="Show All"
                        font=big8
                        toolTipCaption="When checked, always show all args\n\nIf unchecked, hide args that are exceeded the op arg count"
                        padLeft=8
                        />

              <FloatParamTicks id=fp_tickmodulo
                               name="TickMod"
                               dir=LEFT
                               align=centerY
                               value=1 min=1 max=3072 rangeEditable=0
                               reset=12
                               stepFine=1 stepCoarse=1
                               precision=0
                               textAlign=RIGHT
                               toolTipCaption="Tick Modulo\n\n (note) limits processing calls in non-event driven mode (every \"nth\" tick)"
                               cols=3
                               padLeft=8

                               />

              <CheckBox id=cb_event
                        name="Event"
                        dir=LEFT
                        caption="Event"
                        font=big8
                        toolTipCaption="When checked, process entries only when new input event is available (\"event\" mode)\n\nWhen unchecked, process entries each tick (\"idle\" mode)"
                        padLeft=8
                        />

              <CheckBox id=cb_poly
                        name="Poly"
                        dir=LEFT
                        caption="Poly"
                        font=big8
                        toolTipCaption="When checked, process all entries for each polyphonic note or pressure event\n\nWhen unchecked, process all entries only once per tick (e.g. software LFOs)\n\n (note) for non-note-events, set src hold mode to infinite (hinf) to prevent event deletion in polyphonic passes"
                        padLeft=20
                        />

              <Button id=bt_script
                      name="Script"
                      dir=LEFT
                      align=centerY
                      caption="Script"
                      font=big7
                      toolTipCaption="Init entry from script\n\n (note) lctrl-f\n\n (note) hold-click to edit pattern script (lctrl-e)\n\n (note) lctrl-lshift-e or lctrl-p inits pattern script from current pattern and edits it"
                      labelButton=1
                      hold=1
                      padLeft=15
                      />

              <Button id=bt_paste
                      name="Paste"
                      dir=LEFT
                      align=centerY
                      caption="Paste"
                      font=big7
                      toolTipCaption="Paste clipboard to selected entry (lshift-v)\n\n (note) hold-click or lctrl-INSERT shows Ctl.Clipboard menu\n (note) copy ctl port+type to A / B clipboard via lctrl-u / lctrl-lshift-u in Tracker editor"
                      labelButton=1
                      hold=1
                      padLeft=15
                      sizeGroup=sgx_bt3
                      />

              <Button id=bt_copy
                      name="Copy"
                      dir=LEFT
                      align=centerY
                      caption="Copy"
                      font=big7
                      toolTipCaption="Copy selected entry to clipboard (lshift-c)"
                      labelButton=1
                      padLeft=33
                      sizeGroup=sgx_bt3
                      />

              <Button id=bt_movedown
                      name="MoveDown"
                      dir=LEFT
                      align=centerY
                      nocaption=1
                      textPlacement=LEFT
                      icon=movedownh
                      iconMono=movedownh_mono
                      iconPadLeft=4
                      iconPadRight=4
                      toolTipCaption="Move selected entry down (lctrl-DOWN)"
                      labelButton=1
                      padLeft=15
                      sizeGroup=sgx_bt2
                      />

              <Button id=bt_moveup
                      name="MoveUp"
                      dir=LEFT
                      align=centerY
                      nocaption=1
                      textPlacement=LEFT
                      icon=moveuph
                      iconMono=moveuph_mono
                      iconPadLeft=4
                      iconPadRight=4
                      toolTipCaption="Move selected entry up (lctrl-UP)"
                      labelButton=1
                      padLeft=33
                      sizeGroup=sgx_bt2
                      />

              <Button id=bt_delete
                      name="Del"
                      dir=LEFT
                      align=centerY
                      caption="Delete"
                      font=big7
                      textPlacement=LEFT
                      icon=removeh
                      iconMono=removeh_mono
                      toolTipCaption="Delete selected entry (DELETE)"
                      labelButton=1
                      iconPadLeft=4
                      iconPadRight=4
                      padLeft=15
                      sizeGroup=sgx_bt1
                      />

              <Button id=bt_new
                      name="New"
                      dir=LEFT
                      align=centerY
                      textPlacement=LEFT
                      font=big7
                      icon=addh
                      iconMono=addh_mono
                      caption="New"
                      toolTipCaption="Create new entry (INSERT)\n\nHold-click or lshift-INSERT clones current entry"
                      labelButton=1
                      hold=1
                      iconPadLeft=4
                      iconPadRight=4
                      padLeft=8
                      sizeGroup=sgx_bt1
                      />
            </Panel>

            <!-- Name -->
            <Panel dir=BOTTOM align=expand padTop=4 padBottom=4>

              <LayerSwitch id=ls_op dir=LEFT
                           align=centerY
                           >
                <!-- 0: Copy -->
                <Panel>
                  <ComboBox id=cm_op_copy_src
                            name="Src"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Copy source"
                            />

                  <Label dir=LEFT
                         align=centerY
                         caption=""
                         textPlacement=RIGHT
                         font=big7
                         fillBG=0
                         />
                </Panel>

                <!-- 1: Test -->
                <Panel>
                  <Label dir=LEFT caption=") ? 1 : 0"
                         textPlacement=RIGHT
                         align=centerY
                         font=big7
                         fillBG=0
                         />

                  <ComboBox id=cm_op_test_src
                            name="Src"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Test source and store either 1 or 0"
                            />

                  <Label dir=LEFT
                         align=centerY
                         caption="(0 != "
                         font=big7
                         textPlacement=RIGHT
                         fillBG=0
                         />
                </Panel>

                <!-- 2: Not -->
                <Panel>
                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption=") ? 1 : 0"
                         font=big7
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_not_src
                            name="Src"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Test source and store either 1 or 0"
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption="(0 == "
                         font=big7
                         textPlacement=RIGHT
                         />
                </Panel>

                <!-- 3: Add -->
                <Panel>
                  <ComboBox id=cm_op_add_src2
                            name="Src"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Right hand side argument"
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption=" + "
                         font=big7
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_add_src1
                            name="Src"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Left hand side argument"
                            />
                </Panel>

                <!-- 4: Sub -->
                <Panel>
                  <ComboBox id=cm_op_sub_src2
                            name="Src"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Right hand side argument"
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption=" - "
                         font=big7
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_sub_src1
                            name="Src"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Left hand side argument"
                            />
                </Panel>

                <!-- 5: Mix -->
                <Panel>
                  <ComboBox id=cm_op_mix_src4
                            name="Src"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Right hand side argument"
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption=" * "
                         font=big8
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_mix_src3
                            name="Src3"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Right hand side argument"
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption=" + "
                         font=big8
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_mix_src2
                            name="Src2"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Left hand side argument"
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption=" * "
                         font=big8
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_mix_src1
                            name="Src1"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Left hand side argument"
                            />
                </Panel>

                <!-- 6: Mul -->
                <Panel>
                  <ComboBox id=cm_op_mul_mul2
                            name="Mul2"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Multiplier 2"
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption=" * "
                         font=big8
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_mul_mul1
                            name="Mul1"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Multiplier 1"
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption=" * "
                         font=big8
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_mul_src
                            name="Src"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Source value"
                            />
                </Panel>

                <!-- 7: Mul/Div -->
                <Panel>
                  <ComboBox id=cm_op_muldiv_div
                            name="Div"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Divisor"
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption=") / "
                         font=big8
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_muldiv_mul
                            name="Mul"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Multiplier"
                            />

                  <Label dir=LEFT
                         fillBG=0
                         caption=" * "
                         font=big8
                         textPlacement=RIGHT
                         align=centerY
                         />

                  <ComboBox id=cm_op_muldiv_src
                            name="Src"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Source value"
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption=" ("
                         font=big8
                         textPlacement=RIGHT
                         />
                </Panel>

                <!-- 8: Blend -->
                <Panel>
                  <ComboBox id=cm_op_blend_src4
                            name="Src2"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Source 4 (Scaling, 0=Input A, 1=Input B)"
                            padLeft=6
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption=") *"
                         font=big8
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_blend_src3
                            name="Src2"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Source 3\n\n(note) usually this is the same as Input A"
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption="-"
                         font=big8
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_blend_src2
                            name="Src2"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Source 2 (Input B)"
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption=" + ("
                         font=big8
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_blend_src1
                            name="Src1"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Source 1 (Input A)"
                            />
                </Panel>

                <!-- 9: Scale From -->
                <Panel>
                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption="] => [0..1]"
                         font=big8
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_scalefrom_hi
                            name="Div"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Range High"
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption=".."
                         font=big8
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_scalefrom_lo
                            name="Mul"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Range Low"
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption=" ["
                         font=big8
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_scalefrom_src
                            name="Src"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Source"
                            />
                </Panel>

                <!-- 10: Scale To -->
                <Panel>
                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption="]"
                         textPlacement=RIGHT
                         font=big8
                         />

                  <ComboBox id=cm_op_scaleto_hi
                            name="Div"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Range High"
                            />

                  <Label dir=LEFT
                         align=centerY
                         fillBG=0
                         caption=".."
                         font=big8
                         textPlacement=RIGHT
                         />

                  <ComboBox id=cm_op_scaleto_lo
                            name="Mul"
                            dir=LEFT
                            align=centerY
                            options="Dest;Arg 1;Arg 2;Arg 3;Arg 4"
                            font=big7
                            toolTipCaption="Range Low"
