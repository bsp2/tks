IntArray ia; ia.allocAndFill(256, 0);
int sum = 0;
int i = 0;
loop(256)
{
   int r = 0;
   loop(16)
      r += rand(64) - 32;
   r /= 16;
   ia[i++] = r;
   sum += r;
}
while(0 != abs(sum))
{
   trace "sum="+sum;
   int rIdx = rand(256);
   ia[rIdx] = -ia[rIdx];
   sum = 0;
   i = 0;
   loop(256)
      sum += ia[i++];
   trace "xx new sum="+sum;
}

trace "IntArray dither_tbl = "+ia;
