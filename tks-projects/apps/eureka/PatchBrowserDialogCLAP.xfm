<xfm>

  <Panel dir=TOP align=expandx>
    <Panel dir=RIGHT>
      <Button dir=LEFT id=bt_save   caption="Save.." padLeft=10/>
      <Button dir=LEFT id=bt_load   caption="Load.." padLeft=10/>
      <Button dir=LEFT id=bt_revert caption="Revert" />
