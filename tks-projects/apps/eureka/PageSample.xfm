<xfm padding=0;2;0;2>

  <Spacer dir=TOP sizeY=10%/>

  <SplitPane id=spl_sample dir=CENTER split=0.7 vertical=1>
    <!-- Upper -->
    <Panel dir=CENTER align=expand>
      <Panel dir=CENTER align=expand>

        <TabSwitch id=ts_groups
                   name="TSMain"
                   dir=LEFT
                   align=expand
                   fontActiveTab=big8
                   keepParent=1
                   cacheLayout=0
                   setkbdfocusonmouse=1
                   calcsizeonlycurrent=0
                   active=0
                   >

          <!-- 0: Patch -->
          <View caption="Patch">
            <Panel align=expand alpha=255><Panel dir=TOP align=expandx padTop=4>

                <SizeGroupX id=sgx_patch_lb1/>
                <SizeGroupX id=sgx_patch_lb2/>
                <SizeGroupX id=sgx_patch_lb3/>
                <SizeGroupX id=sgx_patch_lb4/>
                <SizeGroupX id=sgx_patch_lb5/>
                <SizeGroupX id=sgx_patch_freq_vol/>
                <SizeGroupX id=sgx_patch_pb_pan/>

                <!-- Name / Type / Poly -->
                <Panel dir=BOTTOM align=expandx>

                  <Panel dir=RIGHT align=baseline>
                    <ComboBox id=cm_prg
                              dir=LEFT
                              align=baseline
                              name="Prg"
                              options="-;1;2;.."
                              toolTipCaption="MIDI program number"
                              arrow=0
                              padLeft=0
                              />
                    <Button dir=LEFT
                            id=bt_prg_assign
                            align=baseline
                            labelButton=1
                            caption="MIDI:"
                            textPlacement=RIGHT
                            fillBG=0
                            toolTipCaption="Click to assign unused MIDI program number"
                            padLeft=2
                            />

                  </Panel>

                  <!--
                  <ComboBox id=cm_syn
                            dir=LEFT
                            align=baseline
                            toolTipCaption="Synthesis type\n\n-: Regular sample, no synthesis\n\nCycle: enable 'Cycle' softsynth (procedural waveform mode)\n\n (note) waveform will not be saved, just its size\n (note) use Cycle patches to fill waveform (see Synth dialog)"
                            options="-;Cycle"
                            arrow=0
                            padLeft=2
                            />
                  <Label dir=LEFT
                         align=baseline
                         fillBG=0
                         caption="Synth:"
                         textPlacement=RIGHT
                         padLeft=6
                         />
                  -->
                  <FloatParam id=fp_audio_bpm
                              dir=LEFT
                              align=baseline
                              value=0 min=70 max=160
                              maxLimit=500 minLimit=0 rangeEditable=1
                              nominalMin=70 nominalMax=140
                              mousePrecision=2 precision=2 step=0.1
                              reset=0;90;125;140
                              unit="bpm"
                              zeroText="-"
                              cols=4
                              toolTipCaption="Audio-Clip beats per minute (BPM)"
                              padLeft=0
                              />
                  <CheckBox id=cb_audio
                            dir=LEFT
                            align=baseline
                            caption="A"
                            toolTipCaption="Audio clip waveform\n\n (note) e.g. created by sample timeline\n (note) UI hint, e.g. for filtering sample lists"
                            padLeft=4
                            setkbdfocusonmouse=0
                            />
                  <ComboBox id=cm_src_track
                            dir=LEFT
                            align=baseline
                            name="SrcTrk"
                            options="-;1;2;.."
                            optionSizeReserve="99"
                            toolTipCaption="Source track\n\n (note) for procedural samples that capture the output of a track\n\n (note) also requires Sample MIDI sequence data to be uploaded from Synergy"
                            arrow=0
                            padLeft=2
                            />
                  <CheckBox id=cb_procedural
                            dir=LEFT
                            align=baseline
                            caption="Proc"
                            toolTipCaption="Procedural waveform\n\nUnchecked: Regular sample data (saved with project)\n\nChecked: procedural waveform, e.g. for 'Cycle' softsynth\n (note) waveform will not be saved, just its size\n (note) use Cycle patches to fill waveform (see Synth dialog)"
                            padLeft=2
                            setkbdfocusonmouse=0
                            />

                  <FloatParam id=fp_maxvoices
                              dir=LEFT
                              align=baseline
                              value=1 min=1 max=256 rangeMaxEditable=0 precision=0 step=1
                              reset=1;32
                              cols=1
                              toolTipCaption="Maximum Number of Voices (all zones combined)"
                              padLeft=0
                              />
                  <Label dir=LEFT
                         align=baseline
                         fillBG=0
                         caption="Poly:"
                         textPlacement=RIGHT
                         padLeft=2
                         />

                  <CheckBox id=cb_persistent
                            dir=LEFT
                            align=baseline
                            nocaption=1
                            toolTipCaption="When checked, mark sample as persistent and never delete it when removing unreferenced samples"
                            padLeft=1
                            setkbdfocusonmouse=0
                            />

                  <TextField id=tf_name
                             dir=LEFT
                             align=baseline
                             cols=20
                             toolTipCaption="Unique Sample Name\n\n (note) also used as filename prefix when writing .esa and .wav files"
                             />

                  <Label dir=LEFT
                         align=baseline
                         fillBG=0
                         caption="Name:"
                         textPlacement=RIGHT
                         padLeft=0
                         sizeGroup=sgx_patch_lb1
                         />
                </Panel>

                <!-- Description -->
                <Panel dir=BOTTOM align=expandx>

                  <Button id=bt_info
                          dir=RIGHT
                          align=baseline
                          caption="M"
                          toolTipCaption="Edit multi-line sample info\n\n (note) lctrl-SPACE, lctrl-RETURN or RMB click when textfield has focus"
                          padLeft=4
                          setkbdfocusonmouse=0
                          />

                       <Button id=bt_tags
                               dir=RIGHT
                               align=baseline
                               caption="T"
                               toolTipCaption="Edit sample tags\n\n (note) t OR lctrl-t OR '<'\n\n (note) RMB-click or lshift-t opens context menu\n\n (note) hold-click or lctrl-t auto-adds (guesses) tags"
                               provideRightClick=1
                               hold=1
                               padLeft=2
                               setkbdfocusonmouse=0
                               />

                  <TextField id=tf_info
                             align=expandx;baseline
                             provideRightClick=1
                             provideMultiLineEdit=1
                             toolTipCaption="Sample info / description\n\n (note) lctrl-SPACE, lctrl-RETURN or RMB click opens multi-line text editor"
                             />

                  <Label dir=LEFT
                         align=baseline
                         fillBG=0
                         caption="Info:"
                         textPlacement=RIGHT
                         padLeft=0
                         sizeGroup=sgx_patch_lb1
                         />
                </Panel>

                <!-- Transpose / Volume / Pan -->
                <Panel dir=BOTTOM align=expandx padTop=8>

                  <Button id=bt_sample_tuning
                          align=baseline
                          caption="Tune.."
                          toolTipCaption="Edit sample micro-tuning tables"
                          padLeft=4
                          setkbdfocusonmouse=0
                          />

                  <CheckBox id=cb_realloc
                            dir=LEFT
                            align=baseline
                            caption="R"
                            toolTipCaption="When checked, try to reallocate the voice that is already playing the given sample\n\n (note) useful for monophonic, analog-style envelopes"
                            padLeft=0
                            setkbdfocusonmouse=0
                            />

                  <CheckBox id=cb_uniquenotes
                            dir=LEFT
                            align=baseline
                            caption="U"
                            toolTipCaption="When checked, allow only one voice per note key (unique notes)\n\n (note) useful for avoiding phasing issues"
                            padLeft=0
                            setkbdfocusonmouse=0
                            />

                  <Spacer dir=LEFT sizeGroup=sgx_patch_lb5 padLeft=10/>

                  <FloatParam id=fp_sample_rate
                              dir=LEFT
                              align=baseline
                              value=0 min=1 max=384000 rangeMaxEditable=0 precision=3 step=1
                              reset=48000
                              mousePrecision=0
                              toolTipCaption="Sample Rate"
                              cols=3
                              padLeft=0
                              />
                  <Label dir=LEFT
                         align=baseline
                         caption="SR:"
                         textPlacement=RIGHT
                         fillBG=0
                         padLeft=10
                         sizeGroup=sgx_patch_lb4
                         />

                  <Panel dir=LEFT align=baseline sizeGroup=sgx_patch_pb_pan>
                    <FloatParam id=fp_pitchbend_down
                                dir=LEFT
                                align=baseline
                                value=0 min=0 max=48 rangeMaxEditable=0 precision=4 step=1
                                reset=12;0
                                mousePrecision=0
                                toolTipCaption="Pitchbend Down"
                                cols=2
                                padLeft=0
                                />
                    <FloatParam id=fp_pitchbend_up
                                dir=LEFT
                                align=baseline
                                value=0 min=0 max=48 rangeMaxEditable=0 precision=4 step=1
                                reset=12;0
                                mousePrecision=0
                                toolTipCaption="Pitchbend Up"
                                cols=2
                                padLeft=0
                                />
                  </Panel>
                  <Label dir=LEFT
                         align=baseline
                         caption="PB:"
                         textPlacement=RIGHT
                         fillBG=0
                         padLeft=10
                         sizeGroup=sgx_patch_lb3
                         />

                  <FloatParam id=fp_freq_rand
                              dir=LEFT
                              align=baseline
                              value=0 min=0 max=48 rangeMaxEditable=0 precision=5 step=0.01
                              reset=0;0.03
                              mousePrecision=3
                              cols=3
                              toolTipCaption="Frequency randomization amount\n\n (note) only applied when zone solo mode is disabled"
                              padLeft=0
                              />
                  <Label dir=LEFT
                         align=baseline
                         caption="R:"
                         textPlacement=RIGHT
                         fillBG=0
                         padLeft=10
                         sizeGroup=sgx_patch_lb2
                         />

                  <Panel dir=LEFT align=baseline sizeGroup=sgx_patch_freq_vol>
                    <FloatParam id=fp_transpose_cents
                                dir=LEFT
                                align=baseline
                                value=0 min=-100 max=100 rangeMaxEditable=0 precision=0 step=1
                                toolTipCaption="Transpose: Cents"
                                cols=2
                                padLeft=0
                                />
                    <FloatParam id=fp_transpose_semi
                                dir=LEFT
                                align=baseline
                                value=0 min=-12 max=12 rangeMaxEditable=0 precision=0 step=1
                                cols=2
                                toolTipCaption="Transpose: Semitones"
                                padLeft=0
                                />
                    <FloatParam id=fp_transpose_oct
                                dir=LEFT
                                align=baseline
                                value=0 min=-4 max=4 rangeMaxEditable=0 precision=0 step=1
                                cols=2
                                toolTipCaption="Transpose: Octave"
                                padLeft=0
                                />
                  </Panel>
                  <Label dir=LEFT
                         align=baseline
                         caption="Freq:"
                         textPlacement=RIGHT
                         fillBG=0
                         padLeft=0
                         sizeGroup=sgx_patch_lb1
                         />
                </Panel>

                <Panel dir=BOTTOM align=expandx padTop=4>

                  <FloatParam id=fp_sampleoff_rand
                              dir=LEFT
                              align=baseline
                              value=0 min=0 max=1 rangeMaxEditable=0 precision=5 stepCoarse=0.001 stepFine=0.0001
                              reset=0;0.2
                              mousePrecision=3
                              cols=3
                              display=percent
                              toolTipCaption="Sample Start Randomization Amount (all zones)\n\n (note) only applied when zone solo mode is disabled"
                              padLeft=0
                              />
                  <Label dir=LEFT
                         align=baseline
                         caption="OffR:"
                         textPlacement=RIGHT
                         fillBG=0
                         padLeft=10
                         sizeGroup=sgx_patch_lb5
                         />

                  <FloatParam id=fp_pan_rand
                              dir=LEFT
                              align=baseline
                              value=0 min=0 max=1 rangeMaxEditable=0 precision=5 step=0.01
                              reset=0;0.3
                              mousePrecision=3
                              cols=3
                              display=percent
                              toolTipCaption="Panning randomization amount\n\n (note) only applied when zone solo mode is disabled"
                              padLeft=0
                              />
                  <Label dir=LEFT
                         align=baseline
                         caption="R:"
                         textPlacement=RIGHT
                         fillBG=0
                         padLeft=10
                         sizeGroup=sgx_patch_lb4
                         />

                  <Panel dir=LEFT align=baseline sizeGroup=sgx_patch_pb_pan>
                    <FloatParam id=fp_pan
                                dir=LEFT
                                align=baseline
                                value=0 min=-1 max=1 rangeMaxEditable=0 precision=3 step=0.05
                                reset=0;0.5;-0.5
                                presets=-1;-0.75;-0.5;-0.25;0;0.25;0.5;0.75;1
                                cols=3
                                display=percent
                                toolTipCaption="Panning"
                                padLeft=0
                                />
                  </Panel>
                  <Label dir=LEFT
                         align=baseline
                         caption="Pan:"
                         textPlacement=RIGHT
                         fillBG=0
                         padLeft=10
                         sizeGroup=sgx_patch_lb3
                         />

                  <FloatParam id=fp_volume_rand
                              dir=LEFT
                              align=baseline
                              value=0 min=0 max=1 rangeMaxEditable=0 precision=5 step=0.01
                              reset=0;0.3
                              mousePrecision=3
                              cols=3
                              display=percent
                              toolTipCaption="Volume randomization amount\n\n (note) only applied when zone solo mode is disabled"
                              padLeft=0
                              />
                  <Label dir=LEFT
                         align=baseline
                         caption="R:"
                         textPlacement=RIGHT
                         fillBG=0
                         padLeft=10
                         sizeGroup=sgx_patch_lb2
                         />

                  <FloatParam id=fp_volume
                              dir=LEFT
                              align=baseline
                              value=0 min=-99 max=0 rangeMaxEditable=0 precision=3 step=0.25
                              reset=0;-12
                              unit=dB
                              cols=3
                              toolTipCaption="Volume (dB)"
                              padLeft=0
                              sizeGroup=sgx_patch_freq_vol
                              />
                  <Label dir=LEFT
                         align=baseline
                         caption="Vol:"
                         textPlacement=RIGHT
                         fillBG=0
                         padLeft=0
                         sizeGroup=sgx_patch_lb1
                         />
                </Panel>

                <Panel dir=BOTTOM padTop=4>
                  <!-- Initial / default performance controllers -->

                  <CheckBox id=cb_sustain
                            dir=LEFT
                            align=baseline
                            caption=""
                            toolTipCaption="When checked, handle sustain pedal events in ModSample player\n\nWhen unchecked, leave it to sample zone mod matrix ('Sustain Pedal' source)"
                            padLeft=8
                            setkbdfocusonmouse=0
                            />

                  <CheckBox id=cb_perfctlfreeze
                            dir=LEFT
                            align=baseline
                            caption=""
                            toolTipCaption="When checked, freeze performance controllers at note-off"
                            padLeft=4
                            setkbdfocusonmouse=0
                            />

                  <!-- General Purpose #4 -->
                  <FloatParam id=fp_perf_gp4
                              dir=LEFT
                              align=baseline
                              value=-1 min=-1 max=127 precision=3 step=1
                              mousePrecision=3
                              negativeText="-"
                              cols=2
                              display=hex
                              toolTipCaption="Initial CC#019 General Purpose 4 performance controller value\n\n (note) >= 0 means \"set value when sample is selected\"\n (note) \'-\' means \"keep current ctl value\""
                              padLeft=0
                              />

                  <Button id=bt_perf_gp4
                          dir=LEFT
                          align=baseline
                          labelButton=1
                          setkbdfocusonmouse=0
                          innerPadRight=0
                          toolTipCaption="Toggle decimal / hexadecimal controller display\n\n (note) decimal mode has increased precision"
                          caption="GP4:"
                          textPlacement=RIGHT
                          fillBG=1
                          padLeft=0
                          />

                  <!-- General Purpose #3 -->
                  <FloatParam id=fp_perf_gp3
                              dir=LEFT
                              align=baseline
                              value=-1 min=-1 max=127 precision=3 step=1
                              mousePrecision=3
                              negativeText="-"
                              cols=2
                              display=hex
                              toolTipCaption="Initial CC#018 General Purpose 3 performance controller value\n\n (note) >= 0 means \"set value when sample is selected\"\n (note) \'-\' means \"keep current ctl value\""
                              padLeft=0
                              />

                  <Button id=bt_perf_gp3
                          dir=LEFT
                          align=baseline
                          labelButton=1
                          setkbdfocusonmouse=0
                          innerPadRight=0
                          toolTipCaption="Toggle decimal / hexadecimal controller display\n\n (note) decimal mode has increased precision"
                          caption="GP3:"
                          textPlacement=RIGHT
                          fillBG=1
                          padLeft=0
                          />

                  <!-- General Purpose #2 -->
                  <FloatParam id=fp_perf_gp2
                              dir=LEFT
                              align=baseline
                              value=-1 min=-1 max=127 precision=3 step=1
                              mousePrecision=3
                              negativeText="-"
                              cols=2
                              display=hex
                              toolTipCaption="Initial CC#017 General Purpose 2 performance controller value\n\n (note) >= 0 means \"set value when sample is selected\"\n (note) \'-\' means \"keep current ctl value\""
                              padLeft=0
                              />

                  <Button id=bt_perf_gp2
                          dir=LEFT
                          align=baseline
                          labelButton=1
                          setkbdfocusonmouse=0
                          innerPadRight=0
                          toolTipCaption="Toggle decimal / hexadecimal controller display\n\n (note) decimal mode has increased precision"
                          caption="GP2:"
                          textPlacement=RIGHT
                          fillBG=1
                          padLeft=0
                          />

                  <!-- General Purpose #1 -->
                  <FloatParam id=fp_perf_gp1
                              dir=LEFT
                              align=baseline
                              value=-1 min=-1 max=127 precision=3 step=1
                              mousePrecision=3
                              negativeText="-"
                              cols=2
                              display=hex
                              toolTipCaption="Initial CC#016 General Purpose 1 performance controller value\n\n (note) >= 0 means \"set value when sample is selected\"\n (note) \'-\' means \"keep current ctl value\""
                              padLeft=0
                              />

                  <Button id=bt_perf_gp1
                          dir=LEFT
                          align=baseline
                          labelButton=1
                          setkbdfocusonmouse=0
                          innerPadRight=0
                          toolTipCaption="Toggle decimal / hexadecimal controller display\n\n (note) decimal mode has increased precision"
                          caption="GP1:"
                          textPlacement=RIGHT
                          fillBG=1
                          padLeft=0
                          />

                  <!-- Expression Pedal -->
                  <FloatParam id=fp_perf_ex
                              dir=LEFT
                              align=baseline
                              value=-1 min=-1 max=127 precision=3 step=1
                              mousePrecision=3
                              negativeText="-"
                              cols=2
                              display=hex
                              toolTipCaption="Initial CC#011 Expression Pedal performance controller value\n\n (note) >= 0 means \"set value when sample is selected\"\n (note) \'-\' means \"keep current ctl value\""
                              padLeft=0
                              />

                  <Button id=bt_perf_ex
                          dir=LEFT
                          align=baseline
                          labelButton=1
                          setkbdfocusonmouse=0
                          innerPadRight=0
                          toolTipCaption="Toggle decimal / hexadecimal controller display\n\n (note) decimal mode has increased precision"
                          caption="EX:"
                          textPlacement=RIGHT
                          fillBG=1
                          padLeft=0
                          />

                  <!-- Foot Control -->
                  <FloatParam id=fp_perf_fc
                              dir=LEFT
                              align=baseline
                              value=-1 min=-1 max=127 precision=3 step=1
                              mousePrecision=3
                              negativeText="-"
                              cols=2
                              display=hex
                              toolTipCaption="Initial CC#004 Foot Control performance controller value\n\n (note) >= 0 means \"set value when sample is selected\"\n (note) \'-\' means \"keep current ctl value\""
                              padLeft=0
                              />

                  <Button dir=LEFT
                          id=bt_perf_fc
                          align=baseline
                          labelButton=1
                          setkbdfocusonmouse=0
                          innerPadRight=0
                          toolTipCaption="Toggle decimal / hexadecimal controller display\n\n (note) decimal mode has increased precision"
                          caption="FC:"
                          textPlacement=RIGHT
                          fillBG=1
                          padLeft=0
                          />

                  <!-- Breath Control -->
                  <FloatParam id=fp_perf_bc
                              dir=LEFT
                              align=baseline
                              value=-1 min=-1 max=127 precision=3 step=1
                              mousePrecision=3
                              negativeText="-"
                              cols=2
                              display=hex
                              toolTipCaption="Initial CC#002 Breath Control performance controller value\n\n (note) >= 0 means \"set value when sample is selected\"\n (note) \'-\' means \"keep current ctl value\""
                              padLeft=0
                              />

                  <Button dir=LEFT
                          id=bt_perf_bc
                          align=baseline
                          labelButton=1
                          setkbdfocusonmouse=0
                          innerPadRight=0
                          toolTipCaption="Toggle decimal / hexadecimal controller display\n\n (note) decimal mode has increased precision"
                          caption="BC:"
                          textPlacement=RIGHT
                          fillBG=1
                          padLeft=0
                          />

                  <!-- Modwheel -->
                  <FloatParam id=fp_perf_mw
                              dir=LEFT
                              align=baseline
                              value=-1 min=-1 max=127 precision=3 step=1
                              mousePrecision=3
                              negativeText="-"
                              cols=2
                              display=hex
                              toolTipCaption="Initial CC#001 Mod Wheel performance controller value\n\n (note) >= 0 means \"set value when sample is selected\"\n (note) \'-\' means \"keep current ctl value\""
                              padLeft=0
                              />

                  <Button id=bt_perf_mw
                          dir=LEFT
                          align=baseline
                          labelButton=1
                          setkbdfocusonmouse=0
                          innerPadRight=0
                          toolTipCaption="Toggle decimal / hexadecimal controller display\n\n (note) decimal mode has increased precision"
                          caption="MW:"
                          textPlacement=RIGHT
                          fillBG=1
                          padLeft=0
                          sizeGroup=sgx_patch_lb1
                          />

                </Panel>

            </Panel></Panel>
          </View>


          <!-- 1: ReInit -->
          <View caption="ReInit">
            <Panel align=expand><Panel dir=TOP padTop=4>

                <!-- Reinit -->
                <Panel dir=BOTTOM>
                  <Button id=bt_reinit_drum
                          dir=LEFT
                          align=baseline
                          caption="Drum"
                          toolTipCaption="Post-Record Re-Init Drum Sample\n\n (note) - set patch level to -3dB\n (note) - set patch+zone polyphony to 1\n (note) - set vol-ramp-up to 0\n (note) - set env release time to 20\n (note) - set zone LM/HM to 0/128\n (note) - normalize waveform\n (note) - auto-add sample tags\n\n (note) lctrl-x d"
                          setkbdfocusonmouse=0
                          padLeft=20
                          style=flat
                          />

                  <CheckBox id=cb_reinit_loop
                            dir=LEFT
                            align=baseline
                            caption="Loop"
                            toolTipCaption="When checked, enable sample loops in re-initialized zones"
                            padLeft=4
                            setkbdfocusonmouse=0
                            />

                  <Button id=bt_reinit_zones
                          dir=LEFT
                          align=baseline
                          caption="Re-Init"
                          toolTipCaption="Auto-Split and Re-Initialize Zones"
                          setkbdfocusonmouse=0
                          padLeft=10
                          style=flat
                          />

                  <FloatParam id=fp_grid_size
                              dir=LEFT
                              align=baseline
                              value=1 min=1 max=128 rangeMaxEditable=0 precision=0 step=1
                              cols=2
                              toolTipCaption="Grid size"
                              padLeft=0
                              />

                  <ComboBox id=cm_type
                            dir=LEFT
                            align=baseline
